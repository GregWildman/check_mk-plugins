--=========================================================                                      
-- TS Pre-provisioning Table
--=========================================================
icomWmacBsSfProvTsTable  OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacBsSfProvTsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table contains Terminal Station provisioning objects (service
        bundle profiles associated to Terminal Stations). One Terminal Station
        can be associated with up to 5 service bundle profiles."    
    ::= { icomWmacBsObjects  8 }

icomWmacBsSfProvTsEntry OBJECT-TYPE 
    SYNTAX       IcomWmacBsSfProvTsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "Each entry holds the association between a Terminal Station and a 
         service bundle profile."
    INDEX  { icomWmacBsId, 
             icomWmacBsTsId, 
             icomWmacBsSfProvTsSrvBundleProfId } 
    ::={ icomWmacBsSfProvTsTable 1 }

IcomWmacBsSfProvTsEntry ::= SEQUENCE {
    icomWmacBsSfProvTsSrvBundleProfId    Unsigned32,
    icomWmacBsSfProvTsRowStatus          RowStatus
}

icomWmacBsSfProvTsSrvBundleProfId   OBJECT-TYPE 
    SYNTAX                          Unsigned32 (1..32768)
    MAX-ACCESS                      not-accessible
    STATUS                          current
    DESCRIPTION  
        "Service bundle profile index. This index uniquely identifies 
        an entry in icomWmacBsSfProvServBundleProfTable. The default 
        service bundle profile index equals to 1."
    ::= { icomWmacBsSfProvTsEntry 1 }

icomWmacBsSfProvTsRowStatus  OBJECT-TYPE 
    SYNTAX                   RowStatus
    MAX-ACCESS               read-create
    STATUS                   current
    DESCRIPTION  
        "This object is used to create a new row or modify or delete
        an existing row in this table." 
    ::= { icomWmacBsSfProvTsEntry 2 }

--=========================================================                                      
-- Service Bundle Profile Table
--=========================================================
icomWmacBsSfProvServBundleProfIndexNext OBJECT-TYPE
    SYNTAX      Unsigned32 (0|2..32768)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "This object contains an unused value for icomWmacBsSfProvServBundleProfIndex in
        the icomWmacBsSfProvServBundleProfEntry, or a zero to indicate that none exist."
    ::= { icomWmacBsObjects  9 }

