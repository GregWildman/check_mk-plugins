    icomWmacBsPerfTsStats15MinIntervalNumber          Unsigned32,
    icomWmacBsPerfTsStats15MinIntervalEndTime         IcomDateTimeType,
    icomWmacBsPerfTsStats15MinIntervalElapsedTime     TimeInterval,
    icomWmacBsPerfTsStats15MinIntervalSuspect         TruthValue,
    icomWmacBsPerfTsStats15MinIntervalInService       Counter64,
    icomWmacBsPerfTsStats15MinIntervalOutOfService    Counter64
}

icomWmacBsPerfTsStats15MinIntervalNumber OBJECT-TYPE
    SYNTAX       Unsigned32 (1..96)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 15-minute interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomWmacBsPerfTsStats15MinIntervalEntry 1 }

