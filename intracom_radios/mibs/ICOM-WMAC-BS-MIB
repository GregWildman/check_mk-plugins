--=========================================================
-- icomWmacBsPerfTsStatsEnable
--=========================================================

icomWmacBsPerfTsStatsEnable OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
       "This object enables/disables the collection of coarse-grain
        historical performance statistics for the terminal stations with
        a 15-minute granularity.

        If value is set to 'true(1)', then collection of historical
        performance statistics is enabled.

        If value is set to 'false(2)', then collection of historical
        performance statistics is disabled. Any existing historical
        performance statistics will be discarded.

        This object is persistent, and its value must be retained across
        reinitializations of the management system.
       "
    DEFVAL { true }
    ::= { icomWmacBsPerfObjects 6 }

-- -------------------------------------------------------------
-- icomWmacBsPerfTsStatsCurrentTable
-- -------------------------------------------------------------

icomWmacBsPerfTsStatsCurrentTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IcomWmacBsPerfTsStatsCurrentEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "This table provides coarse-grain terminal station performance
        statistics for the current 15-minute interval. All table
        statistics are reset to zero after the completion of each
        15-minute measurement interval.
       "
    ::= { icomWmacBsPerfObjects 7 }

icomWmacBsPerfTsStatsCurrentEntry OBJECT-TYPE
    SYNTAX       IcomWmacBsPerfTsStatsCurrentEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "An entry (conceptual row) in the
        icomWmacBsPerfTsStatsCurrentTable.
       "
    INDEX  { icomWmacBsId,
             icomWmacBsTsId
           }
    ::= { icomWmacBsPerfTsStatsCurrentTable 1 }

IcomWmacBsPerfTsStatsCurrentEntry ::= SEQUENCE {
    icomWmacBsPerfTsStatsCurrentElapsedTime     TimeInterval,
    icomWmacBsPerfTsStatsCurrentSuspect         TruthValue,
    icomWmacBsPerfTsStatsCurrentInService       Counter64,
    icomWmacBsPerfTsStatsCurrentOutOfService    Counter64
}

icomWmacBsPerfTsStatsCurrentElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that the current measurement interval has
        been running for, in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomWmacBsPerfTsStatsCurrentEntry 1 }

icomWmacBsPerfTsStatsCurrentSuspect OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomWmacBsPerfTsStatsCurrentEntry 2 }

icomWmacBsPerfTsStatsCurrentInService OBJECT-TYPE
    SYNTAX       Counter64
    UNITS        "seconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Time in service in the current 15-minute interval.
        "
    ::= { icomWmacBsPerfTsStatsCurrentEntry 3 }

icomWmacBsPerfTsStatsCurrentOutOfService OBJECT-TYPE
    SYNTAX       Counter64
    UNITS        "seconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Time out of service in the current 15-minute interval.
        "
    ::= { icomWmacBsPerfTsStatsCurrentEntry 4 }

-- -------------------------------------------------------------
-- icomWmacBsPerfTsStats15MinIntervalTable
-- -------------------------------------------------------------

icomWmacBsPerfTsStats15MinIntervalTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IcomWmacBsPerfTsStats15MinIntervalEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "This table provides coarse-grain terminal station performance
        statistics for historical (completed) 15-minute intervals.

        This table will have a maximum of 96 entries of completed 15-minute
        intervals, to provide historical statistics for one day.
       "
    ::= { icomWmacBsPerfObjects 8 }

