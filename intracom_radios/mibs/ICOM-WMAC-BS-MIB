--=========================================================                                      
-- Wmac BsTs Status Table
--=========================================================
icomWmacBsTsStatusTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacBsTsStatusEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table provides BS-side TS-specific current 
        status parameters."    
    ::= { icomWmacBsObjects  4 }

icomWmacBsTsStatusEntry OBJECT-TYPE 
    SYNTAX       IcomWmacBsTsStatusEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        " "
    AUGMENTS  { icomWmacBsTsCfgEntry } 
    ::={ icomWmacBsTsStatusTable 1 }

IcomWmacBsTsStatusEntry ::= SEQUENCE {                   
    icomWmacBsTsStatusConnectivity        INTEGER,
    icomWmacBsTsStatusMonitoring          INTEGER,
    icomWmacBsTsStatusUpTime              TimeTicks,     
    icomWmacBsTsStatusLastEntryTime       TimeStamp,
    icomWmacBsTsStatusDiuc                WmacDlPhysicalMode,
    icomWmacBsTsStatusUiuc                WmacUlPhysicalMode,
    icomWmacBsTsStatusUlTxPower           Integer32,
    icomWmacBsTsStatusUlTxPowerHeadroom   Integer32, 
    icomWmacBsTsStatusDlRssi              Integer32,
    icomWmacBsTsStatusDlSnr               Integer32,
    icomWmacBsTsStatusDlFecStress         Integer32,
    icomWmacBsTsStatusUlRssi              Integer32,
    icomWmacBsTsStatusUlSnr               Integer32,
    icomWmacBsTsStatusUlFecStress         Integer32,
    icomWmacBsTsStatusInfo                INTEGER,
    icomWmacBsTsStatusAddressType         InetAddressType, 
    icomWmacBsTsStatusIpAddress           InetAddress,
    icomWmacBsTsStatusIpNetmask           InetAddress,
    icomWmacBsTsStatusMngVlanId           Unsigned32,
    icomWmacBsTsStatusDistance            Integer32
}

icomWmacBsTsStatusConnectivity   OBJECT-TYPE 
    SYNTAX                       INTEGER { initialization(1),
                                           ranging(2),
                                           rangingSuccess(3),
                                           removeReq(4),
                                           connected(5),
                                           ranged(6),
                                           invalid(7)        
                                         }
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "Connectivity state. Valid values are:
            DOWN (INIT): TS is disconnected
            RANGING: TS is being ranged
            RANGING SUCCESS: Ranging procedure successful
            REM_REQ: A TS remove procedure is under way
            CONNECTED: TS is connected
            STATE_RANGED: TS is connected but has not reached
                          specified PHY modulations
            STATE_INVALID: Invalid state." 
    ::= { icomWmacBsTsStatusEntry 1 }

icomWmacBsTsStatusMonitoring   OBJECT-TYPE 
    SYNTAX                     INTEGER { initialization(1),
                                         startReq(2),
                                         stopReq(3), 
                                         active(4),
                                         monitoringFail(5),
                                         invalidState(6)
                                       }
    MAX-ACCESS                 read-only
    STATUS                     current
    DESCRIPTION  
        "When icomWmacBsTsCfgReportingInterval is non-zero, the BS will establish monitoring
        with the TS. Note that if monitoring fails for any reason, the BS will drop
        the TS. In the standard case were the TS remains in the BS client (TS) list,
        the TS will be re-ranged. Monitoring states are:
            INIT: Monitoring disabled.
            START_REQ: BS has requested monitoring from the TS
            STOP_REQ: The BS has requested from the TS to stop sending monitoring messages.
            ACTIVE: BS is regularly receiving monitoring messages from the TS.
            MONITORING_FAIL: Monitoring fails. 
            STATE_INVALID: Invalid state. " 
    ::= { icomWmacBsTsStatusEntry 2 }

icomWmacBsTsStatusUpTime   OBJECT-TYPE 
    SYNTAX                 TimeTicks
    MAX-ACCESS             read-only
    STATUS                 current
    DESCRIPTION  
        "Up time since the last time the TS entered the network. Available only in
        CONNECTED state." 
    ::= { icomWmacBsTsStatusEntry 3 }

icomWmacBsTsStatusLastEntryTime  OBJECT-TYPE 
    SYNTAX                       TimeStamp
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "Last time the TS entered the network." 
    ::= { icomWmacBsTsStatusEntry 4 }

icomWmacBsTsStatusDiuc   OBJECT-TYPE 
