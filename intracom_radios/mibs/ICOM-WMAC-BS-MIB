REVISION  "201312180000Z"  -- dtzou: December 18, 2013
 DESCRIPTION
     "o Maximum length for profile names is 32 octets. Default values changed

      o Value range added and description updated for icomWmacBsTsCfgMinUiuc, 
        icomWmacBsTsCfgMaxUiuc, icomWmacBsTsCfgMinDiuc, icomWmacBsTsCfgMaxDiuc

      o Default value for icomWmacBsTsCfgMacAddress removed

      o icomWmacTsId renamed to icomWmacBsTsId

      o icomWmacBsSfProvServProfTable renamed to icomWmacBsSfProvServBundleProfTable
        including all the related MOs

      o icomWmacBsTsDlPhyResetStats, icomWmacBsTsUlPhyResetStats MOs added
        in icomWmacBsTsDlPhyStatsEntry and icomWmacBsTsUlPhyStatsEntry respectively

      o Valid value range added and description updated for icomWmacBsTsUlPhyUiuc
        and icomWmacBsTsDlPhyDiuc

      o icomWmacBsSfProvTsEntry modified to allow 1-to-many relationship between 
        TS and service (bundle) profile

      o Default value for icomWmacBsSfProvServFlowProfMandatory changed to false

      o icomWmacBsSfProvServClassTable renamed to icomWmacBsSfProvServClassProfTable

      o icomWmacBsSfProvClsRuleTable renamed to icomWmacBsSfProvClsRuleProfTable
 
      o Default value for icomWmacBsSfProvServFlowProfClsRuleProfId changed to 1

      o Default value for icomWmacBsSfProvServFlowProfServClassProfId changed to 1  
     "

REVISION  "201311200000Z"  -- dtzou: November 20, 2013
 DESCRIPTION
     "Updated icomWmacBsSfProvServFlowProfTable and icomWmacBsSfProvServClassTable."

REVISION  "201310140000Z"
 DESCRIPTION
     "Initial version."

::= { intracom 112 }

icomWmacBsNotifications     OBJECT IDENTIFIER ::= { icomWmacBsMIB 0 }
icomWmacBsObjects           OBJECT IDENTIFIER ::= { icomWmacBsMIB 1 }
icomWmacBsPerfObjects       OBJECT IDENTIFIER ::= { icomWmacBsMIB 2 }
icomWmacBsConformance       OBJECT IDENTIFIER ::= { icomWmacBsMIB 3 }

icomWmacBsCompliances       OBJECT IDENTIFIER ::= { icomWmacBsConformance 1 }
icomWmacBsGroups            OBJECT IDENTIFIER ::= { icomWmacBsConformance 2 }


WmacClassifierBitMap ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION     
        "A bit of of this object is set to 1 if the parameter
        indicated by the comment was present in the classifier
        encoding, and 0 otherwise.
        Note: that BITS are encoded most significant bit first,
             so that if e.g. bits 6 and 7 are set, this object is
             encoded as the octet string '030000'H."
    SYNTAX     BITS { destMacAddr(0),
                      srcMacAddr(1),
                      ethernetProtocol(2),
                      userPriority(3),
                      cvlanId(4),
                      svlanId(5),
                      ipTos(6),
                      ipSrcAddr(7),
                      ipDestAddr(8),
                      ipProtocol(9),
                      mplsExp(10),  
                      ipv6SrcAddr(11),
                      ipv6DestAddr(12),
                      ipv6TrafficClass(13),
                      ipv6FlowLabel(14),
                      srcPort(15),
                      destPort(16)
                     }  

WmacSchedulingType ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION     
            "The scheduling service provided by a service class 
             for a service flow. If the parameter is omitted from
             a QOS Parameter Set, this object takes the value of
             bestEffort(1)."
        SYNTAX      INTEGER { bestEffort(1),
                              nonRealTimePollingService(2),
                              realTimePollingService(3),
                              unsolicitedGrantService(4)}

WmacDlPhysicalMode::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION     
            "The allowed value of downlink physical modes."
        SYNTAX      INTEGER { mode4qamlow(1),
                              mode4qammed(2),
                              mode4qamhigh(3),
                              mode16qamlow(4),
                              mode16qamhigh(5),
                              mode64qam(6),
                              mode128qam(7),
                              mode256qam(8),
                              mode512qam(9),
                              mode1024qamlow(10),
                              mode1024qamhigh(11)  
                            }

WmacUlPhysicalMode::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION     
            "The allowed value of uplink physical modes."
        SYNTAX      INTEGER { mode4qamlow(1),
                              mode4qammed(2),
                              mode4qamhigh(3),
                              mode16qamlow(4),
                              mode16qamhigh(5),
                              mode64qam(6),
                              mode128qam(7),
                              mode256qam(8)
                            }
--=========================================================  
-- Base Station SNMP Trap objects
--=========================================================
icomWmacBsTraps        OBJECT IDENTIFIER ::= { icomWmacBsNotifications 0 }
icomWmacBsTrapControl  OBJECT IDENTIFIER ::= { icomWmacBsNotifications 1 }

icomWmacBsTsStatusNotificationTrap NOTIFICATION-TYPE
    OBJECTS {icomWmacBsTsStatusConnectivity, icomWmacBsTsStatusInfo}
    STATUS current
    DESCRIPTION
        "This trap reports the status of a Terminal Station (e.g. connected, disconnected)."
    ::= { icomWmacBsTraps 1 }

icomWmacBsTsPhyModeChangeTrap  NOTIFICATION-TYPE
    OBJECTS {icomWmacBsTsStatusDiuc, icomWmacBsTsStatusUiuc}
    STATUS current
    DESCRIPTION
        "This trap reports the PHY mode change of a Terminal Station."
    ::= { icomWmacBsTraps 2 }

icomWmacBsSfOperStatusNotificationTrap  NOTIFICATION-TYPE
    OBJECTS {icomWmacCmnServFlowOperStatus, icomWmacCmnServFlowFailReason}
    STATUS current
    DESCRIPTION
        "This trap reports the operational state of a service flow
        (as described in the state machine of icomWmacCmnServFlowOperStatus) and 
		the reason of change."
    ::= { icomWmacBsTraps 3 }

--=========================================================  
-- Trap Control
--=========================================================
icomWmacBsTrapControlRegister    OBJECT-TYPE
    SYNTAX BITS { icomWmacBsTsStatusNotification(0),
                  icomWmacBsTsPhyModeChange(1),
                  icomWmacBsSfOperStatusNotification(2)
                }
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
        "The object is used to enable Base Station traps. From left to right, the 
         set bit indicates the corresponding trap is enabled."
	::= { icomWmacBsTrapControl 1 }

--=========================================================                                      
-- Wmac Bs Configuration Table
--=========================================================
icomWmacBsCfgTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacBsCfgEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table provides the parameters to configure the 
        Base Station."    
    ::= { icomWmacBsObjects  1 }

icomWmacBsCfgEntry OBJECT-TYPE 
    SYNTAX       IcomWmacBsCfgEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        " "
    INDEX  { icomWmacBsId } 
    ::={ icomWmacBsCfgTable 1 }

IcomWmacBsCfgEntry ::= SEQUENCE { 
    icomWmacBsId                            Integer32,                  
    icomWmacBsCfgMaximumRange               Integer32,
    icomWmacBsCfgRngInvTimer                Unsigned32,
    icomWmacBsCfgRngChRepTimer              Unsigned32,
    icomWmacBsCfgRngInvRetries              Unsigned32,
    icomWmacBsCfgRngAdjustRetries           Unsigned32,
    icomWmacBsCfgRngInvSuccessRetries       Unsigned32,
    icomWmacBsCfgMonChRepReqRetries         Unsigned32,
    icomWmacBsCfgMonChRepTimer              Unsigned32,
    icomWmacBsCfgMonCheckTimer              Unsigned32,
    icomWmacBsCfgRngReceivedPowerTarget     Integer32,
    icomWmacBsCfgRngReceivedPowerThres      Integer32,
    icomWmacBsCfgRngTimingThres             Integer32, 
    icomWmacBsCfgRngFrequencyThres          Integer32,
    icomWmacBsCfgSfRspTimer                 Unsigned32,
    icomWmacBsCfgSfReqRetries               Unsigned32, 
    icomWmacBsCfgSfHoldTimer                Unsigned32
}

icomWmacBsId   OBJECT-TYPE 
    SYNTAX       Integer32 (1)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "Base Station ID. It is restricted to 1 instance." 
    ::= { icomWmacBsCfgEntry 1 }

icomWmacBsCfgMaximumRange  OBJECT-TYPE 
    SYNTAX       Integer32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Maximum coverage range of the BS." 
    ::= { icomWmacBsCfgEntry 2 }

icomWmacBsCfgRngInvTimer   OBJECT-TYPE 
    SYNTAX       Unsigned32
    UNITS        "milliseconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Time to wait after an unanswered Ranging Invitation, before sending
        the next one." 
    DEFVAL {500}
    ::= { icomWmacBsCfgEntry 3 }

icomWmacBsCfgRngChRepTimer    OBJECT-TYPE 
    SYNTAX       Unsigned32
    UNITS        "milliseconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Time to wait for a Channel Report message, after the ranging
        procedure succeeds." 
    DEFVAL {1000}
    ::= { icomWmacBsCfgEntry 4 }

icomWmacBsCfgRngInvRetries   OBJECT-TYPE 
    SYNTAX       Unsigned32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Maximum number of unanswered Ranging Invitations (ranging retries)."
    DEFVAL {10} 
    ::= { icomWmacBsCfgEntry 5 }

icomWmacBsCfgRngAdjustRetries   OBJECT-TYPE 
    SYNTAX       Unsigned32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Maximum number of adjustments sent to a TS during ranging."
    DEFVAL {10} 
    ::= { icomWmacBsCfgEntry 6 }

icomWmacBsCfgRngInvSuccessRetries   OBJECT-TYPE 
    SYNTAX       Unsigned32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Maximum number of Ranging Invitations with status success
        sent to the TS after ranging succeeds." 
    DEFVAL {5}
    ::= { icomWmacBsCfgEntry 7 }

icomWmacBsCfgMonChRepReqRetries   OBJECT-TYPE 
    SYNTAX       Unsigned32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Maximum Number of Channel Report Request messages sent to the TS
        during monitoring operations." 
    DEFVAL {5}
    ::= { icomWmacBsCfgEntry 8 }

icomWmacBsCfgMonChRepTimer    OBJECT-TYPE 
    SYNTAX       Unsigned32
    UNITS        "milliseconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Time to wait for a Channel Report as a response to a Channel Report
        Request during monitoring operations."
    DEFVAL {5000} 
    ::= { icomWmacBsCfgEntry 9 }

icomWmacBsCfgMonCheckTimer    OBJECT-TYPE 
    SYNTAX       Unsigned32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "When monitoring is enabled, the BS will check for received Channel
        Reports at T5 x RI."
    DEFVAL {5} 
    ::= { icomWmacBsCfgEntry 10 }

icomWmacBsCfgRngReceivedPowerTarget   OBJECT-TYPE 
    SYNTAX       Integer32
    UNITS        "0.5 dBm"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Received power target during ranging."
    DEFVAL {10} 
    ::= { icomWmacBsCfgEntry 11 }

icomWmacBsCfgRngReceivedPowerThres   OBJECT-TYPE 
    SYNTAX       Integer32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "If the difference between the measured received power of a ranging burst 
        (RNG Channel Report) and the Ranging Receive Power Target is less 
        (in absolute value) than this threshold, the burst is considered acceptable 
        in terms of power (i.e. no further power adjustments will be announced)." 
    DEFVAL {4}
    ::= { icomWmacBsCfgEntry 12 }

icomWmacBsCfgRngTimingThres   OBJECT-TYPE 
    SYNTAX       Integer32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "If the measured timing offset of a ranging burst (RNG Channel Report)
        is less (in absolute value) than this threshold, the burst is considered 
        acceptable in terms of timing (i.e. no further timing adjustments will be 
        announced)."
    DEFVAL {2} 
    ::= { icomWmacBsCfgEntry 13 }

icomWmacBsCfgRngFrequencyThres   OBJECT-TYPE 
    SYNTAX       Integer32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "If the measured frequency offset of a ranging burst (RNG Channel
        Report) is less (in absolute value) than this threshold, the
        burst is considered acceptable in terms of frequency (i.e. no
        frequency adjustment will be announced)." 
    DEFVAL {50}
    ::= { icomWmacBsCfgEntry 14 }

icomWmacBsCfgSfRspTimer    OBJECT-TYPE 
    SYNTAX       Unsigned32
    UNITS        "milliseconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Time to wait for an SF management response message after sending a
        corresponding request message."
    DEFVAL {1000} 
    ::= { icomWmacBsCfgEntry 15 }

icomWmacBsCfgSfReqRetries    OBJECT-TYPE 
    SYNTAX       Unsigned32
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Maximum number of unanswered SF management request messages sent
        to the TS."
    DEFVAL {5} 
    ::= { icomWmacBsCfgEntry 16 }

icomWmacBsCfgSfHoldTimer   OBJECT-TYPE 
    SYNTAX       Unsigned32
    UNITS        "milliseconds"
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Time to wait for any pending messages to be sent, before closing 
        an SF transaction (add, remove, modify)." 
    DEFVAL {5000}
    ::= { icomWmacBsCfgEntry 17 }

--=========================================================                                      
-- Wmac Bs Statistics Table
--=========================================================
icomWmacBsStatsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacBsStatsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table models the Base Station statistics."    
    ::= { icomWmacBsObjects  2 }

icomWmacBsStatsEntry OBJECT-TYPE 
    SYNTAX       IcomWmacBsStatsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        " "
    AUGMENTS  { icomWmacBsCfgEntry } 
    ::={ icomWmacBsStatsTable 1 }

IcomWmacBsStatsEntry ::= SEQUENCE {
    icomWmacBsStatsReset     TruthValue,
    icomWmacBsStatsGbiSent   OCTET STRING
}

icomWmacBsStatsReset    OBJECT-TYPE 
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION  
        "Reset BS statistics." 
    DEFVAL { false }
    ::= { icomWmacBsStatsEntry 1 }

icomWmacBsStatsGbiSent OBJECT-TYPE 
    SYNTAX       OCTET STRING
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "General Broadcast Information messages sent." 
    ::= { icomWmacBsStatsEntry 2 }

--=========================================================                                      
-- Wmac BsTs Configuration Table
--=========================================================
icomWmacBsTsCfgTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacBsTsCfgEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table provides BS-side TS-specific configuration
        parameters."    
    ::= { icomWmacBsObjects  3 }

icomWmacBsTsCfgEntry OBJECT-TYPE 
    SYNTAX       IcomWmacBsTsCfgEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        " "
    INDEX  { icomWmacBsId, icomWmacBsTsId } 
    ::={ icomWmacBsTsCfgTable 1 }

IcomWmacBsTsCfgEntry ::= SEQUENCE {                   
    icomWmacBsTsId                          Integer32,
    icomWmacBsTsCfgAdminStatus              INTEGER,
    icomWmacBsTsCfgMacAddress               MacAddress,
    icomWmacBsTsCfgMinDiuc                  WmacDlPhysicalMode,     
    icomWmacBsTsCfgMaxDiuc                  WmacDlPhysicalMode,
    icomWmacBsTsCfgMinUiuc                  WmacUlPhysicalMode,
    icomWmacBsTsCfgMaxUiuc                  WmacUlPhysicalMode,
    icomWmacBsTsCfgMinUlTxPower             Integer32,
    icomWmacBsTsCfgMaxUlTxPower             Integer32, 
    icomWmacBsTsCfgReportingInterval        Integer32,
    icomWmacBsTsCfgResetTs                  TruthValue,
    icomWmacBsTsCfgDefAnySFStatus           TruthValue,
    icomWmacBsTsCfgUiucLimit                TruthValue,
    icomWmacBsTsCfgRowStatus                RowStatus,
    icomWmacBsTsCfgDefAnySFDlMaxQueueDepth0 Integer32,
    icomWmacBsTsCfgDefMngSFMinRate          Integer32,
    icomWmacBsTsCfgDefMngSFMaxRate          Integer32
}

icomWmacBsTsId   OBJECT-TYPE 
    SYNTAX       Integer32 (1..30)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "Terminal Station ID." 
    ::= { icomWmacBsTsCfgEntry 1 }

icomWmacBsTsCfgAdminStatus    OBJECT-TYPE 
    SYNTAX                    INTEGER { down(1),
                                        up(2) }
    MAX-ACCESS                read-create
    STATUS                    current
    DESCRIPTION  
        "TS administration status (values UP/DOWN). Changing status to DOWN for
        a connected TS, also disconnects the TS." 
    DEFVAL { down }
    ::= { icomWmacBsTsCfgEntry 2 }

icomWmacBsTsCfgMacAddress  OBJECT-TYPE 
    SYNTAX                 MacAddress
    MAX-ACCESS             read-create
    STATUS                 current
    DESCRIPTION  
        "TS IEEE 802.3 MAC address used for its air interface."
    ::= { icomWmacBsTsCfgEntry 3 }

icomWmacBsTsCfgMinDiuc  OBJECT-TYPE 
    SYNTAX              WmacDlPhysicalMode
    MAX-ACCESS          read-create
    STATUS              current
    DESCRIPTION  
        "Minimum allowed DL physical mode." 
    DEFVAL { mode4qamlow } 
    ::= { icomWmacBsTsCfgEntry 4 }

icomWmacBsTsCfgMaxDiuc  OBJECT-TYPE 
    SYNTAX              WmacDlPhysicalMode
    MAX-ACCESS          read-create
    STATUS              current
    DESCRIPTION  
        "Maximum allowed DL physical mode."  
    DEFVAL { mode1024qamhigh }
    ::= { icomWmacBsTsCfgEntry 5 }

icomWmacBsTsCfgMinUiuc  OBJECT-TYPE 
    SYNTAX              WmacUlPhysicalMode
    MAX-ACCESS          read-create
    STATUS              current
    DESCRIPTION  
        "Minimum allowed UL physical mode." 
    DEFVAL { mode4qamlow }
    ::= { icomWmacBsTsCfgEntry 6 }

icomWmacBsTsCfgMaxUiuc  OBJECT-TYPE 
