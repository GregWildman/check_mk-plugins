--=========================================================                                      
-- Common Classifier Criteria Table
--=========================================================
icomWmacCmnClsTable   OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacCmnClsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table contains the status of packet classification criteria associated with 
        created service flows. It provides one row for each packet classification rule."    
    ::= { icomWmacCmnObjects  4 }

icomWmacCmnClsEntry OBJECT-TYPE 
    SYNTAX       IcomWmacCmnClsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "An entry is indexed by the ComponentID, PeerID, SFID, and icomWmacCmnClsRuleId."
    INDEX  { icomWmacCmnComponentId, 
             icomWmacCmnPeerId, 
             icomWmacCmnSfId, 
             icomWmacCmnClsRuleId 
           } 
    ::={ icomWmacCmnClsTable 1 }

IcomWmacCmnClsEntry ::= SEQUENCE {
    icomWmacCmnClsRuleId             Integer32,
    icomWmacCmnClsDestMacAddr        MacAddress,
    icomWmacCmnClsDestMacMask        MacAddress,
    icomWmacCmnClsSrcMacAddr         MacAddress,
    icomWmacCmnClsSrcMacMask         MacAddress,
    icomWmacCmnClsEnetProtocol       Integer32,
    icomWmacCmnClsUserPriLow         Integer32,
    icomWmacCmnClsUserPriHigh        Integer32, 
    icomWmacCmnClsCvlanIdLow         Unsigned32,
    icomWmacCmnClsCvlanIdHigh        Unsigned32,
    icomWmacCmnClsSvlanIdLow         Unsigned32, 
    icomWmacCmnClsSvlanIdHigh        Unsigned32,
    icomWmacCmnClsIpTosLow           Integer32,
    icomWmacCmnClsIpTosHigh          Integer32,
    icomWmacCmnClsIpTosMask          Integer32,
    icomWmacCmnClsAddressType        InetAddressType,
    icomWmacCmnClsIpSrcAddr          InetAddress, 
    icomWmacCmnClsIpSrcMask          InetAddress,
    icomWmacCmnClsIpDestAddr         InetAddress,
    icomWmacCmnClsIpDestMask         InetAddress,
    icomWmacCmnClsIpProtocol         Integer32,
    icomWmacCmnClsMplsExp            Integer32, 
    icomWmacCmnClsIpv6TrfClassLow    Integer32,
    icomWmacCmnClsIpv6TrfClassHigh   Integer32,
    icomWmacCmnClsIpv6FlowLabelLow   WmacIpv6FlowLabel,
    icomWmacCmnClsIpv6FlowLabelHigh  WmacIpv6FlowLabel,
    icomWmacCmnClsSrcPortStart       Integer32,   
    icomWmacCmnClsSrcPortEnd         Integer32,
    icomWmacCmnClsDestPortStart      Integer32,
    icomWmacCmnClsDestPortEnd        Integer32,
    icomWmacCmnClsBitMap             WmacClassifierBitMap,
    icomWmacCmnClsAssociatedPhsi     Integer32
}

icomWmacCmnClsRuleId          OBJECT-TYPE 
    SYNTAX                    Integer32 (1..32768)
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION  
        "Classifier rule index."
    ::= { icomWmacCmnClsEntry 1 }

icomWmacCmnClsDestMacAddr   OBJECT-TYPE 
    SYNTAX                      MacAddress
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object specifies the value of the Ethernet destination
        MAC address required for packets to match this rule. An 
        Ethernet packet matches the rule when the packet's destination 
        MAC address bitwise ANDed with the icomWmacSfBsProvClsRuleDestMacMask 
        value equals the icomWmacSfBsProvClsRuleDestMacAddr value." 
    ::= { icomWmacCmnClsEntry 2 }

icomWmacCmnClsDestMacMask   OBJECT-TYPE 
    SYNTAX                      MacAddress
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object specifies which bits of a packet's Ethernet 
        destination MAC address are compared to match this rule.
        An Ethernet packet matches the rule when the packets destination
        MAC address bitwise ANDed with the icomWmacSfBsProvClsRuleDestMacMask
        value equals the icomWmacSfBsProvClsRuleDestMacAddr value."
    ::= { icomWmacCmnClsEntry 3 }

icomWmacCmnClsSrcMacAddr    OBJECT-TYPE 
    SYNTAX                      MacAddress
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object specifies the value of the Ethernet source MAC 
        address required for packets to match this rule. An Ethernet 
        packet matches the rule when the packet's source MAC address 
        bitwise ANDed with the icomWmacSfBsProvClsRuleSrcMacMask value
        equals the icomWmacSfBsProvClsRuleSrcMacAddr value." 
    ::= { icomWmacCmnClsEntry 4 }

icomWmacCmnClsSrcMacMask      OBJECT-TYPE 
    SYNTAX                        MacAddress
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "This object specifies which bits of a packet's Ethernet 
        source MAC address are compared to match this rule. An 
        Ethernet packet matches the rule when the packet's source
        MAC address bitwise ANDed with the icomWmacSfBsProvClsRuleSrcMacMask
        value equals the icomWmacSfBsProvClsRuleSrcMacAddr value." 
    ::= { icomWmacCmnClsEntry 5 }

icomWmacCmnClsEnetProtocol    OBJECT-TYPE 
    SYNTAX                        Integer32
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "This object specifies the 16-bit value of the EtherType that 
        the packet must match in order to match the rule. If the 
        Ethernet frame contains an IEEE 802.1Q tag header 
        (i.e., EtherType 0x8100), this object applies to the 
        embedded EtherType field within the IEEE 802.1Q header." 
    ::= { icomWmacCmnClsEntry 6 }

icomWmacCmnClsUserPriLow    OBJECT-TYPE 
    SYNTAX                      Integer32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object applies only to Ethernet frames using the 
        IEEE 802.1Q tag header (indicated with EtherType 0x8100). 
        Such frames include a 16-bit Tag that contains a 3 bit 
        Priority field and a 12 bit VLAN number. Tagged Ethernet 
        packets must have a 3-bit Priority field within the range
        of icomWmacSfBsProvClsRuleUserPriLow and 
        icomWmacSfBsProvClsRuleUserPriHigh in order to match this rule." 
    ::= { icomWmacCmnClsEntry 7 }

icomWmacCmnClsUserPriHigh   OBJECT-TYPE 
    SYNTAX                      Integer32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object applies only to Ethernet frames using the 
        IEEE 802.1Q tag header (indicated with EtherType 0x8100). 
        Such frames include a 16-bit Tag that contains a 3 bit 
        Priority field and a 12 bit VLAN number. Tagged Ethernet 
        packets must have a 3-bit Priority field within the range 
        of icomWmacSfBsProvClsRuleUserPriLow and 
        icomWmacSfBsProvClsRuleUserPriHigh in order to match this rule." 
    ::= { icomWmacCmnClsEntry 8 }

icomWmacCmnClsCvlanIdLow    OBJECT-TYPE 
    SYNTAX                      Unsigned32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object applies to Ethernet frames that carry a C-VLAN ID. 
        Ethernet frames must have a 12-bit C-VLAN ID field within the 
        range of icomWmacSfBsProvClsRuleCvlanIdLow and 
        icomWmacSfBsProvClsRuleCvlanIdHigh in order to match this rule."
    ::= { icomWmacCmnClsEntry 9 }

icomWmacCmnClsCvlanIdHigh   OBJECT-TYPE 
    SYNTAX                      Unsigned32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object applies to Ethernet frames that carry a C-VLAN ID. 
        Ethernet frames must have a 12-bit C-VLAN ID field within the range 
        of icomWmacSfBsProvClsRuleCvlanIdLow and icomWmacSfBsProvClsRuleCvlanIdHigh
        in order to match this rule." 
    ::= { icomWmacCmnClsEntry 10 }

icomWmacCmnClsSvlanIdLow    OBJECT-TYPE 
    SYNTAX                      Unsigned32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object applies to Ethernet frames that carry a S-VLAN ID. 
        Ethernet frames must have a 12-bit S-VLAN ID field within the range 
        of icomWmacSfBsProvClsRuleSvlanIdLow and icomWmacSfBsProvClsRuleSvlanIdHigh
        in order to match this rule." 
    ::= { icomWmacCmnClsEntry 11 }

icomWmacCmnClsSvlanIdHigh   OBJECT-TYPE 
    SYNTAX                      Unsigned32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object applies to Ethernet frames that carry a S-VLAN ID. Ethernet
        frames must have a 12-bit S-VLAN ID field within the range of 
        icomWmacSfBsProvClsRuleSvlanIdLow and icomWmacSfBsProvClsRuleSvlanIdHigh 
        in order to match this rule." 
    ::= { icomWmacCmnClsEntry 12 }

icomWmacCmnClsIpTosLow    OBJECT-TYPE 
    SYNTAX                    Integer32
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION  
        "This object specifies the value of the IPv4 ToS field required for 
        packets to match this rule. Packets must have an IPv4 ToS field within
        the range of icomWmacSfBsProvClsRuleIpTosLow and icomWmacSfBsProvClsRuleIpTosHigh
        in order to match this rule."
    ::= { icomWmacCmnClsEntry 13 }

icomWmacCmnClsIpTosHigh     OBJECT-TYPE 
    SYNTAX                      Integer32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "This object specifies the value of the IPv4 ToS field required for packets
        to match this rule. Packets must have an IPv4 ToS field within the range of 
        icomWmacSfBsProvClsRuleIpTosLow and icomWmacSfBsProvClsRuleIpTosHigh in order
        to match this rule." 
    ::= { icomWmacCmnClsEntry 14 }

icomWmacCmnClsIpTosMask   OBJECT-TYPE 
    SYNTAX                    Integer32
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION  
        "This object specifies the ToS mask. The ToS mask bitwise ANDed with a 
        packet's IPv4 ToS field must be in the range of icomWmacSfBsProvClsRuleIpTosLow 
        and icomWmacSfBsProvClsRuleIpTosHigh in order to match the rule." 
    ::= { icomWmacCmnClsEntry 15 }

icomWmacCmnClsAddressType     OBJECT-TYPE 
    SYNTAX                        InetAddressType
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "A value that represents a type of Internet address." 
    ::= { icomWmacCmnClsEntry 16 }

icomWmacCmnClsIpSrcAddr    OBJECT-TYPE 
    SYNTAX                     InetAddress
    MAX-ACCESS                 read-only
    STATUS                     current
    DESCRIPTION  
        "This object specifies the value of the IP Source Address required for packets
        to match this rule. An IP packet matches the rule when the packet IP source 
        address bitwise ANDed with the icomWmacSfBsProvClsRuleIpSrcMask value equals the 
        icomWmacSfBsProvClsRuleIpSrcAddr value." 
    ::= { icomWmacCmnClsEntry 17 }

icomWmacCmnClsIpSrcMask   OBJECT-TYPE 
    SYNTAX                    InetAddress
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION  
        "This object specifies which bits of a packet's IP Source Address that are 
        compared to match this rule. An IP packet matches the rule when the packet 
        source address bitwise ANDed with the icomWmacSfBsProvClsRuleIpSrcMask value 
        equals the icomWmacSfBsProvClsRuleIpSrcAddr value." 
    ::= { icomWmacCmnClsEntry 18 }

icomWmacCmnClsIpDestAddr   OBJECT-TYPE 
    SYNTAX                     InetAddress
    MAX-ACCESS                 read-only
    STATUS                     current
    DESCRIPTION  
        "This object specifies the value of the IP Destination Address required for 
        packets to match this rule. An IP packet matches the rule when the packet IP 
        destination address bitwise ANDed with the icomWmacSfBsProvClsRuleIpDestMask 
        value equals the icomWmacSfBsProvClsRuleIpDestAddr value."
    ::= { icomWmacCmnClsEntry 19 }

icomWmacCmnClsIpDestMask   OBJECT-TYPE 
    SYNTAX                     InetAddress
    MAX-ACCESS                 read-only
    STATUS                     current
    DESCRIPTION  
        "This object specifies which bits of a packet's IP Destination Address that are
        compared to match this rule. An IP packet matches the rule when the packet 
        destination address bitwise ANDed with the icomWmacSfBsProvClsRuleIpDestMask 
        value equals the icomWmacSfBsProvClsRuleIpDestAddr value."
    ::= { icomWmacCmnClsEntry 20 }

icomWmacCmnClsIpProtocol     OBJECT-TYPE 
    SYNTAX                       Integer32 (0..255)
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "This object specifies the IP protocol field a packet should have to match this 
         rule." 
    ::= { icomWmacCmnClsEntry 21 }

icomWmacCmnClsMplsExp        OBJECT-TYPE 
    SYNTAX                       Integer32
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "This object specifies the MPLS EXP field a packet should have in the outer  
        MPLS lable to match this rule." 
    ::= { icomWmacCmnClsEntry 22 }

icomWmacCmnClsIpv6TrfClassLow   OBJECT-TYPE 
    SYNTAX                          Integer32
    MAX-ACCESS                      read-only
    STATUS                          current
    DESCRIPTION  
        "This object specifies the value of the IPv6 traffic class field required
        for packets to match this rule. Packets must have an IPv6 traffic class field
        within the range of icomWmacSfBsProvClsRuleIpv6TrfClassLow and 
        icomWmacSfBsProvClsRuleIpv6TrfClassHigh in order to match this rule."
    ::= { icomWmacCmnClsEntry 23 }

icomWmacCmnClsIpv6TrfClassHigh  OBJECT-TYPE 
    SYNTAX                          Integer32
    MAX-ACCESS                      read-only
    STATUS                          current
    DESCRIPTION  
        "This object specifies the value of the IPv6 traffic class field required 
        for packets to match this rule. Packets must have an IPv6 traffic class field
        within the range of icomWmacSfBsProvClsRuleIpv6TrfClassLow and 
        icomWmacSfBsProvClsRuleIpv6TrfClassHigh in order to match this rule."
    ::= { icomWmacCmnClsEntry 24 }

icomWmacCmnClsIpv6FlowLabelLow   OBJECT-TYPE 
    SYNTAX                           WmacIpv6FlowLabel
    MAX-ACCESS                       read-only
    STATUS                           current
    DESCRIPTION  
        "This object specifies the value of the IPv6 flow label field required for 
        packets to match this rule. Packets must have an IPv6 flow label field within
        the range of icomWmacSfBsProvClsRuleIpv6FlowLabelLow and 
        icomWmacSfBsProvClsRuleIpv6FlowLabelHigh."
    ::= { icomWmacCmnClsEntry 25 }

icomWmacCmnClsIpv6FlowLabelHigh   OBJECT-TYPE 
    SYNTAX                            WmacIpv6FlowLabel
    MAX-ACCESS                        read-only
    STATUS                            current
    DESCRIPTION  
        "This object specifies the value of the IPv6 flow label field required for 
        packets to match this rule. Packets must have an IPv6 flow label field within 
        the range of icomWmacSfBsProvClsRuleIpv6FlowLabelLow and 
        icomWmacSfBsProvClsRuleIpv6FlowLabelHigh."
    ::= { icomWmacCmnClsEntry 26 }

icomWmacCmnClsSrcPortStart    OBJECT-TYPE 
    SYNTAX                        Integer32
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "This object specifies the low end inclusive range of TCP/UDP source port 
        numbers to which a packet is compared. This object is irrelevant for non-TCP/UDP
        IP packets. If the referenced parameter is not present in a classifier, this 
        object reports the value of 0."
    ::= { icomWmacCmnClsEntry 27 }

icomWmacCmnClsSrcPortEnd      OBJECT-TYPE 
    SYNTAX                        Integer32
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "This object specifies the high end inclusive range of TCP/UDP source port 
        numbers to which a packet is compared. This object is irrelevant for non-TCP/UDP 
        IP packets. If the referenced parameter is not present in a classifier, this 
        object reports the value of 65535."
    ::= { icomWmacCmnClsEntry 28 }

icomWmacCmnClsDestPortStart   OBJECT-TYPE 
    SYNTAX                        Integer32
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "This object specifies the low end inclusive range of TCP/UDP destination port 
        numbers to which a packet is compared. If the referenced parameter is not 
        present in a classifier, this object reports the value of 0."
    ::= { icomWmacCmnClsEntry 29 }

icomWmacCmnClsDestPortEnd     OBJECT-TYPE 
    SYNTAX                        Integer32
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "This object specifies the high end inclusive range of TCP/UDP destination 
        port numbers to which a packet is compared. If the referenced parameter is 
        not present in a classifier, this object reports the value of 65535."
    ::= { icomWmacCmnClsEntry 30 }

icomWmacCmnClsBitMap      OBJECT-TYPE 
    SYNTAX                    WmacClassifierBitMap
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION  
        "This object indicates which parameter encodings were actually present in the
        entry. A bit set to '1' indicates the corresponding classifier encoding is 
        present, and '0' means otherwise."
    ::= { icomWmacCmnClsEntry 31 } 

icomWmacCmnClsAssociatedPhsi    OBJECT-TYPE 
    SYNTAX                          Integer32
    MAX-ACCESS                      read-only
    STATUS                          current
    DESCRIPTION  
        "The Associated PHSI has a value between 1 and 8, which shall mirror the PHSI 
        value of a PHS rule. Packets matching the Packet Classification Rule containing 
        the Associated PHSI parameter shall undergo PHS according to the corresponding PHS 
        rule. A value '0' indicates that no PHS rule is associated with this classifier rule."
    ::= { icomWmacCmnClsEntry 32 }

--=========================================================                                      
-- Common PHS Rule Table
--=========================================================
icomWmacCmnPhsRuleTable   OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacCmnPhsRuleEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table contains PHS rule dictionary entries. This 
        table provides one row for each PHS rule created 
        dynamically by the BS and TS on a given service flow."    
    ::= { icomWmacCmnObjects  5 }

icomWmacCmnPhsRuleEntry OBJECT-TYPE 
    SYNTAX       IcomWmacCmnPhsRuleEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "An entry is indexed by the Component, Peer, SFID, and PHSI."
    INDEX  { icomWmacCmnComponentId, 
             icomWmacCmnPeerId, 
             icomWmacCmnSfId, 
             icomWmacCmnPhsRuleId 
           } 
    ::={ icomWmacCmnPhsRuleTable 1 }

IcomWmacCmnPhsRuleEntry ::= SEQUENCE {
    icomWmacCmnPhsRuleId                 Integer32,
    icomWmacCmnPhsRuleField              OCTET STRING,
    icomWmacCmnPhsRuleMask               OCTET STRING,
    icomWmacCmnPhsRuleSize               Integer32,
    icomWmacCmnPhsRuleVerify             TruthValue
}

icomWmacCmnPhsRuleId   OBJECT-TYPE
    SYNTAX             Integer32 (1..32768)
    MAX-ACCESS         not-accessible
    STATUS             current
    DESCRIPTION 
        "The PHS Index (PHSI) has a value between 1 and 8, which 
        uniquely references the suppressed byte string. The index 
        is unique per service flow. The DL and UL PHSI values are 
        independent of each other."
    ::= { icomWmacCmnPhsRuleEntry 1 }

icomWmacCmnPhsRuleField  OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The PHS field (PHSF) is a string of bytes containing the header 
        information to be suppressed by the sending CS and reconstructed 
        by the receiving CS. The most significant byte of the string 
        corresponds to the first byte of the CS-SDU."
    ::= { icomWmacCmnPhsRuleEntry 2 }

icomWmacCmnPhsRuleMask  OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The PHS mask (PHSM) is an 8-bit mask that indicates which 
        bytes in the PHSF to suppress and which bytes to not suppress. 
        The PHSM allows fields, such as sequence numbers or checksums 
        (which vary in value), to be excluded from suppression with
        the constant bytes around them suppressed. It is encodded as 
        follows: 
            bit 0: 0 = don't suppress the 1st byte of the suppression field 
                   1 = suppress first byte of the suppression field 
            bit 1: 0 = don't suppress the 2nd byte of the suppression field 
                   1 = suppress second byte of the suppression field 
            bit x: 0 = don't suppress the (x+1) byte of the suppression field 
                   1 = suppress (x+1) byte of the suppression field where the 
                       length of the octet string is ceiling (icomWmacCmnPhsRuleSize/8)."             
    ::= { icomWmacCmnPhsRuleEntry 3 }

icomWmacCmnPhsRuleSize  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "byte"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of this field (PHSS) is the total number of bytes
        in the header to be suppressed and then restored in a service 
        flow that uses PHS."
    ::= { icomWmacCmnPhsRuleEntry 4 }

icomWmacCmnPhsRuleVerify     OBJECT-TYPE 
    SYNTAX                     TruthValue
    MAX-ACCESS                 read-only
    STATUS                     current
    DESCRIPTION  
        "The value of this field (PHSV) indicates to the sending 
        entity whether or not the packet header contents are to be 
        verified prior to performing suppression."
    ::= { icomWmacCmnPhsRuleEntry 5 }

