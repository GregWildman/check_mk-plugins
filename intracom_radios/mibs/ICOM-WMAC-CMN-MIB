    icomWmacCmnPerfSfCurrentElapsedTime     TimeInterval,
    icomWmacCmnPerfSfCurrentSuspect         TruthValue,
    icomWmacCmnPerfSfCurrentInService       TimeTicks,
    icomWmacCmnPerfSfCurrentOutService      TimeTicks,
    icomWmacCmnPerfSfCurrentTxQueueDropPkts Counter64,
    icomWmacCmnPerfSfCurrentFbpDropPkts     Counter64,
    icomWmacCmnPerfSfCurrentUlErrPdu        Counter64,
    icomWmacCmnPerfSfCurrentUlErrSdu        Counter64,
    icomWmacCmnPerfSfCurrentUlErrSduFrag    Counter64,
    icomWmacCmnPerfSfCurrentUlErrSduLength  Counter64
}

