        "
    ::= { icomWmacCmnPerfSfCurrentEntry 4 }

icomWmacCmnPerfSfCurrentTxQueueDropPkts OBJECT-TYPE
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Number of SDUs dropped due to congestion in transmit queue in
         the current 15-minute interval.
        "
    ::= { icomWmacCmnPerfSfCurrentEntry 5 }

icomWmacCmnPerfSfCurrentFbpDropPkts OBJECT-TYPE
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
