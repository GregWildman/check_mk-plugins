    icomWmacCmnPerfSfStats15MinIntervalNumber          Unsigned32,
    icomWmacCmnPerfSfStats15MinIntervalEndTime         IcomDateTimeType,
    icomWmacCmnPerfSfStats15MinIntervalElapsedTime     TimeInterval,
    icomWmacCmnPerfSfStats15MinIntervalSuspect         TruthValue,
    icomWmacCmnPerfSfStats15MinIntervalDlNumSdus       Counter64,
    icomWmacCmnPerfSfStats15MinIntervalUlNumSdus       Counter64,
    icomWmacCmnPerfSfStats15MinIntervalDlSduBytes      Counter64,
    icomWmacCmnPerfSfStats15MinIntervalUlSduBytes      Counter64
}

icomWmacCmnPerfSfStats15MinIntervalNumber OBJECT-TYPE
    SYNTAX       Unsigned32 (1..96)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 15-minute interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 1 }

icomWmacCmnPerfSfStats15MinIntervalEndTime OBJECT-TYPE
    SYNTAX       IcomDateTimeType
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
       "The time that this measurement interval ended.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 2 }

icomWmacCmnPerfSfStats15MinIntervalElapsedTime OBJECT-TYPE
    SYNTAX      TimeInterval
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The length of time that this measurement interval ran for,
        in centi-seconds.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 3 }

icomWmacCmnPerfSfStats15MinIntervalSuspect OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
       "This variable indicates if the data for this interval have been
        marked as suspect.

        Value 'true(1)' indicates that the related performance data have
        been marked as suspect.

        Value 'false(2)' indicates that the related performance data
        have not been marked as suspect.
       "
    REFERENCE
       "ITU-T G.7710/Y.1701 clause 10.1.6.1
       "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 4 }

icomWmacCmnPerfSfStats15MinIntervalDlNumSdus OBJECT-TYPE
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Counter of number of SDUs (packets) transferred in the DL
         direction in this completed 15-minute interval.
        "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 5 }

icomWmacCmnPerfSfStats15MinIntervalUlNumSdus OBJECT-TYPE
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Counter of number of SDUs (packets) received in the UL direction
         in this completed 15-minute interval.
        "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 6 }

icomWmacCmnPerfSfStats15MinIntervalDlSduBytes OBJECT-TYPE
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Counter of number of SDU bytes transferred in the DL direction
         in this completed 15-minute interval.
        "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 7 }

icomWmacCmnPerfSfStats15MinIntervalUlSduBytes OBJECT-TYPE
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Counter of number of SDU bytes transferred in the UL direction
