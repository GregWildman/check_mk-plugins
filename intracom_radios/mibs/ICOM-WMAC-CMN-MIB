icomWmacCmnNotifications     OBJECT IDENTIFIER ::= { icomWmacCmnMIB 0 }
icomWmacCmnObjects           OBJECT IDENTIFIER ::= { icomWmacCmnMIB 1 }
icomWmacCmnPerfObjects       OBJECT IDENTIFIER ::= { icomWmacCmnMIB 2 }
icomWmacCmnConformance       OBJECT IDENTIFIER ::= { icomWmacCmnMIB 3 }

icomWmacCmnCompliances       OBJECT IDENTIFIER ::= { icomWmacCmnConformance 1 }
icomWmacCmnGroups            OBJECT IDENTIFIER ::= { icomWmacCmnConformance 2 }


--=========================================================  
-- Textual Conventions
--=========================================================
WmacIpv6FlowLabel ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "1x:"
    STATUS      current
    DESCRIPTION     
        "The value of this field specifies the matching values for
        the IPv6 Flow label field. As the flow label field has a
        length of 20 bits, the first 4 bits of the most significant
        byte shall be set to 0x0 and disregarded."
    SYNTAX      OCTET STRING (SIZE(3))

WmacClassifierBitMap ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION     
        "A bit of of this object is set to 1 if the parameter
        indicated by the comment was present in the classifier
        encoding, and 0 otherwise.
        Note: that BITS are encoded most significant bit first,
              so that if e.g. bits 6 and 7 are set, this object is
              encoded as the octet string '030000'H."
    SYNTAX     BITS { destMacAddr(0),
                      srcMacAddr(1),
                      ethernetProtocol(2),
                      userPriority(3),
                      cvlanId(4),
                      svlanId(5),
                      ipTos(6),
                      ipSrcAddr(7),
                      ipDestAddr(8),
                      ipProtocol(9),
                      mplsExp(10),  
                      ipv6SrcAddr(11),
                      ipv6DestAddr(12),
                      ipv6TrafficClass(13),
                      ipv6FlowLabel(14),
                      srcPort(15),
                      destPort(16)
                     }  

--=========================================================                                      
-- Wmac Common Status Table
--=========================================================
icomWmacCmnStatusTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacCmnStatusEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table provides the status of the WMAC common for BS and TS."    
    ::= { icomWmacCmnObjects  1 }

icomWmacCmnStatusEntry OBJECT-TYPE 
    SYNTAX       IcomWmacCmnStatusEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        " "
    INDEX  { icomWmacCmnComponentId } 
    ::={ icomWmacCmnStatusTable 1 }

IcomWmacCmnStatusEntry ::= SEQUENCE {
    icomWmacCmnComponentId       Integer32,                   
    icomWmacCmnStatusMacAddr     MacAddress,
    icomWmacCmnStatusBsFrameNum  Integer32,
    icomWmacCmnifIndex           InterfaceIndexOrZero,
    icomWmacCmnLEDStatus         INTEGER
}   

icomWmacCmnComponentId  OBJECT-TYPE 
    SYNTAX          Integer32 (1)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION  
        "Base Station:     It is restricted to 1 instance 
         Terminal Station: It is restricted to 1 instance."
    ::= { icomWmacCmnStatusEntry 1 }
   
icomWmacCmnStatusMacAddr  OBJECT-TYPE 
    SYNTAX                MacAddress
    MAX-ACCESS           read-only
    STATUS               current
    DESCRIPTION  
        "IEEE 802.3 MAC address used for the air interface."
    ::= { icomWmacCmnStatusEntry 2 }

icomWmacCmnStatusBsFrameNum OBJECT-TYPE 
    SYNTAX                  Integer32
    MAX-ACCESS              read-only
    STATUS                  current
    DESCRIPTION  
        "Radio frame number." 
    ::= { icomWmacCmnStatusEntry 3 }   

icomWmacCmnifIndex OBJECT-TYPE 
    SYNTAX         InterfaceIndexOrZero
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION  
        "Mapping of the component id in interface entry." 
    ::= { icomWmacCmnStatusEntry 4 }        

icomWmacCmnLEDStatus  OBJECT-TYPE 
    SYNTAX            INTEGER { black(0),
                                red(1),
                                redBlink(2),
                                orange(3),
                                orangeBlink(4),
                                green(5),
                                greenBlink(6)
                              }
    MAX-ACCESS         read-only
    STATUS             current
    DESCRIPTION  
        "This object indicates the LED Indications in accordance to System Status at a 
         Streetnode PmP System.
           For BS System:
			  black: System not operational
			  red: Radio and Modem Alarms are raised.
			  redBlink: Not Used.
			  orange: System Muted. Not Transmitting.
			  orangeBlink: No assigned TSs is synchronized yet.
			  green:  Not all assigned TSs are synchronized yet.
			  greenBlink: All assigned TSs are synchronized.

           For TS System:
			  black: System not operational
			  red: Radio, Modem and / or Antenna Alarms are raised.
              redBlink: Antenna Alignment in Progress
              orange: Reception OK. Not yet Invited.
              orangeBlink: Ranging in Progress.
              green: Not Used.
              greenBlink: TS operates normally." 
    ::= { icomWmacCmnStatusEntry 5 }   

--=========================================================                                      
-- Wmac Common Statistics Table
--=========================================================
icomWmacCmnStatsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacCmnStatsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table provides WMAC statistics common for BS and TS."    
    ::= { icomWmacCmnObjects  2 }

icomWmacCmnStatsEntry OBJECT-TYPE 
    SYNTAX       IcomWmacCmnStatsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This entry is indexed by icomWmacCmnComponentId."
    AUGMENTS  { icomWmacCmnStatusEntry } 
    ::={ icomWmacCmnStatsTable 1 }

IcomWmacCmnStatsEntry ::= SEQUENCE {
    icomWmacCmnStatsTxPkts              Counter64,
    icomWmacCmnStatsTxBytes             Counter64,
    icomWmacCmnStatsRxPkts              Counter64,
    icomWmacCmnStatsRxBytes             Counter64,
    icomWmacCmnStatsHcsErrs             Counter64,
    icomWmacCmnStatsCrcErrs             Counter64,
    icomWmacCmnStatsCidRangeErrs        Counter64,
    icomWmacCmnStatsInvalidMsgTypeErrs  Counter64,
    icomWmacCmnStatsCidDisabledErrs     Counter64,
    icomWmacCmnStatsDecodingErrs        Counter64,
    icomWmacCmnStatsAggrDlSduBitRate    Counter64,
    icomWmacCmnStatsAggrUlSduBitRate    Counter64,
    icomWmacCmnStatsResetCounter        TruthValue,
    icomWmacCmnStatsAggrDlPduBitRate    Counter64,
    icomWmacCmnStatsAggrUlPduBitRate    Counter64
}   

icomWmacCmnStatsTxPkts   OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Number of transport packets transmitted over the wireless
        interface."
      ::= { icomWmacCmnStatsEntry 1 }

icomWmacCmnStatsTxBytes OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Number of transport bytes transmitted over the wireless 
        interface." 
    ::= { icomWmacCmnStatsEntry 2 }   

icomWmacCmnStatsRxPkts OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Number of transport packets received over the wireless
        interface." 
    ::= { icomWmacCmnStatsEntry 3 }

icomWmacCmnStatsRxBytes OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Number of transport bytes received over the wireless 
        interface." 
    ::= { icomWmacCmnStatsEntry 4 }

icomWmacCmnStatsHcsErrs OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "MAC PDU header checksum errors." 
    ::= { icomWmacCmnStatsEntry 5 }

icomWmacCmnStatsCrcErrs OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "MAC PDU CRC errors." 
    ::= { icomWmacCmnStatsEntry 6 }

icomWmacCmnStatsCidRangeErrs OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "MAC PDU received with invalid connection ID errors." 
    ::= { icomWmacCmnStatsEntry 7 }

icomWmacCmnStatsInvalidMsgTypeErrs OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Bad MAC management message type errors." 
    ::= { icomWmacCmnStatsEntry 8 }

icomWmacCmnStatsCidDisabledErrs OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "MAC PDU received on a disabled connection errors." 
    ::= { icomWmacCmnStatsEntry 9 }

icomWmacCmnStatsDecodingErrs OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "MAC message decoding/parsing errors." 
    ::= { icomWmacCmnStatsEntry 10 }

icomWmacCmnStatsAggrDlSduBitRate OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Aggregate downlink SDU bit rate." 
    ::= { icomWmacCmnStatsEntry 11 }

icomWmacCmnStatsAggrUlSduBitRate OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Aggregate uplink SDU bit rate." 
    ::= { icomWmacCmnStatsEntry 12 }

icomWmacCmnStatsResetCounter   OBJECT-TYPE 
    SYNTAX                     TruthValue
    MAX-ACCESS                 read-write
    STATUS                     current
    DESCRIPTION  
        "Reset WMAC statistics."
    DEFVAL { false }
    ::= { icomWmacCmnStatsEntry 13 }

icomWmacCmnStatsAggrDlPduBitRate OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Aggregate downlink PDU bit rate." 
    ::= { icomWmacCmnStatsEntry 14 }

icomWmacCmnStatsAggrUlPduBitRate OBJECT-TYPE 
    SYNTAX       Counter64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  
        "Aggregate uplink PDU bit rate." 
    ::= { icomWmacCmnStatsEntry 15 }

--=========================================================                                      
-- Common Service Flow Table
--=========================================================
icomWmacCmnServFlowTable   OBJECT-TYPE
    SYNTAX       SEQUENCE OF   IcomWmacCmnServFlowEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "This table contains the status of the service flow database. It contains
        service flow managed objects that are common in BS and TS. It reports each 
        service flow (established, failed, or established and failed) between BS and
        TS. It provides one row for each created service flow for a given TSID. 
        When this table is implemented in the BS, the icomWmacCmnPeerId object
        contains the TSID, the reported service flow was created for. 
        In TS, the corresponding value is 1. The icomWmacCmnServBundleProfileId 
        object is valid on BS. In TS, this object returns 0."   
    ::= { icomWmacCmnObjects  3 }

icomWmacCmnServFlowEntry OBJECT-TYPE 
    SYNTAX       IcomWmacCmnServFlowEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
        "An entry is indexed by the componentID, peerID and SFID."
    INDEX  { icomWmacCmnComponentId, 
             icomWmacCmnPeerId, 
             icomWmacCmnSfId 
           } 
    ::={ icomWmacCmnServFlowTable 1 }

IcomWmacCmnServFlowEntry ::= SEQUENCE {
    icomWmacCmnPeerId                    Integer32,
    icomWmacCmnSfId                      Unsigned32,
    icomWmacCmnServFlowCid               Integer32,      
    icomWmacCmnServFlowDirection         INTEGER,
    icomWmacCmnServFlowOperStatus        INTEGER,
    icomWmacCmnServFlowAdminStatus       TruthValue,
    icomWmacCmnServFlowFailReason        INTEGER,
    icomWmacCmnServFlowMandatory         TruthValue,
    icomWmacCmnServFlowTrafficPriority   Integer32,
    icomWmacCmnServFlowMaxSustainedRate  Unsigned32,
    icomWmacCmnServFlowMinReservedRate   Unsigned32,
    icomWmacCmnServFlowGrantInterval     Unsigned32,
    icomWmacCmnServFlowSchedulingType    INTEGER,
    icomWmacCmnServFlowReqTxPolicy       BITS,
    icomWmacCmnServFlowWeight            Integer32, 
    icomWmacCmnServFlowEstablishedTime   TimeStamp,
    icomWmacCmnServFlowQueueType         INTEGER,
    icomWmacCmnServFlowQueueWeight4      Integer32,
    icomWmacCmnServFlowQueueWeight5      Integer32,
    icomWmacCmnServFlowQueueWeight6      Integer32,
    icomWmacCmnServFlowQueueWeight7      Integer32,
    icomWmacCmnServFlowMaxQueueDepth0    Integer32,
    icomWmacCmnServFlowMaxQueueDepth1    Integer32,
    icomWmacCmnServFlowMaxQueueDepth2    Integer32,
    icomWmacCmnServFlowMaxQueueDepth3    Integer32,
    icomWmacCmnServFlowMaxQueueDepth4    Integer32,
    icomWmacCmnServFlowMaxQueueDepth5    Integer32,
    icomWmacCmnServFlowMaxQueueDepth6    Integer32,
    icomWmacCmnServFlowMaxQueueDepth7    Integer32,
    icomWmacCmnServFlowQueueDepth0       Integer32,
    icomWmacCmnServFlowQueueDepth1       Integer32,
    icomWmacCmnServFlowQueueDepth2       Integer32,
    icomWmacCmnServFlowQueueDepth3       Integer32,
    icomWmacCmnServFlowQueueDepth4       Integer32, 
    icomWmacCmnServFlowQueueDepth5       Integer32,
    icomWmacCmnServFlowQueueDepth6       Integer32,
    icomWmacCmnServFlowQueueDepth7       Integer32,
    icomWmacCmnServFlowQueueNumBuffers0  Integer32,
    icomWmacCmnServFlowQueueNumBuffers1  Integer32,
    icomWmacCmnServFlowQueueNumBuffers2  Integer32,
    icomWmacCmnServFlowQueueNumBuffers3  Integer32,
    icomWmacCmnServFlowQueueNumBuffers4  Integer32,
    icomWmacCmnServFlowQueueNumBuffers5  Integer32,
    icomWmacCmnServFlowQueueNumBuffers6  Integer32,
    icomWmacCmnServFlowQueueNumBuffers7  Integer32,
    icomWmacCmnServFlowInService         TimeTicks,
    icomWmacCmnServFlowOutService        TimeTicks,
    icomWmacCmnServBundleProfileId       Integer32,
    icomWmacCmnServFlowProfileId         Integer32,
    icomWmacCmnServFlowMinPhyMode        Integer32
}

icomWmacCmnPeerId             OBJECT-TYPE 
    SYNTAX                    Integer32 (1..30)
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION  
        "Base Station:     PeerId = 1..30, 
         Terminal Station: PeerId = 1."
    ::= { icomWmacCmnServFlowEntry 1 }

icomWmacCmnSfId               OBJECT-TYPE 
    SYNTAX                    Unsigned32 (1..32768)
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION  
        "Service flow ID (SFID). This object uniquely identifies 
         a service flow for a TS. SFID is common in BS and TS.
         For a Terminal Station we can have up to 15 SfIds for DL
         and up to 15 SfIds for UL."
    ::= { icomWmacCmnServFlowEntry 2 }

icomWmacCmnServFlowCid     OBJECT-TYPE 
    SYNTAX                 Integer32
    MAX-ACCESS             read-only
    STATUS                 current
    DESCRIPTION  
        "WMAC Connection ID (CID)."
    ::= { icomWmacCmnServFlowEntry 3 }

icomWmacCmnServFlowDirection  OBJECT-TYPE 
    SYNTAX                    INTEGER { uplink(1),
        				                downlink(2) 
                                      }
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION  
        "Service flow direction (DL/UL)."
    ::= { icomWmacCmnServFlowEntry 4 }

icomWmacCmnServFlowOperStatus    OBJECT-TYPE 
    SYNTAX                       INTEGER {
								invalid(1),
								init(2),
								addreq(3),
								addrsp(4),
								noretries(5),
								holdadd(6),
								holdrem(7),
								deleting(8),
								remreq(9),
								modreq(10),
								modrsp(11),
								established(12),
								disabling(13)
								}
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "The current operational state of the service flow.
         BS SIDE: 
           INIT: Service flow unallocated
           ADD_REQ: SF-ADD-REQ sent
           NO_RETRIES: SF-ADD-REQ retries exhausted
           HOLD_ADD: Waiting for any pending messages during addition
           DELETING: Service flow is being deleted before reaching 
                     ESTABLISHED state
           REM_REQ: SF-REM-REQ sent
           HOLD_REM: Waiting for any pending messages during removal
           MOD_REQ: SF-MOD-REQ sent
           ESTABLISHED: Service flow established
           INVALID: Invalid state
         TS SIDE:
           INIT: No service flow exists
           ADD_RSP: Service flow addition response sent
           HOLD_ADD: Waiting for any pending transaction messages before 
                     closing the addition transaction
           HOLD_REM: Waiting for any pending transaction messages before  
                     closing the removal transaction
           MOD_RSP: Service flow removal response sent
           ESTABLISHED: Service flow established
           DISABLING: Service flow about to be deleted (waiting for any 
                      pending SDUs)
           INVALID: Invalid state."
    ::= { icomWmacCmnServFlowEntry 5 }

icomWmacCmnServFlowAdminStatus    OBJECT-TYPE 
    SYNTAX                        TruthValue
    MAX-ACCESS                    read-write
    STATUS                        current
    DESCRIPTION  
        "The desired state of the service flow (enabled/disabled)."
    ::= { icomWmacCmnServFlowEntry 6 }

icomWmacCmnServFlowFailReason   OBJECT-TYPE 
    SYNTAX                      INTEGER { noFailure(0), 
                                          minPhyModeCrossed(1),
                                          sfResponseTimeout(2) 
                                        }
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION  
        "Reason of failure for the service flow. A service flow may fail either in
         creation phase or operation phase."
    ::= { icomWmacCmnServFlowEntry 7 }

icomWmacCmnServFlowMandatory  OBJECT-TYPE 
    SYNTAX                    TruthValue
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION  
        "Service flow mandatory state (if true and servive flow creation 
        fails, no other subsequent service flows are created)."
    ::= { icomWmacCmnServFlowEntry 8 }

icomWmacCmnServFlowTrafficPriority  OBJECT-TYPE 
    SYNTAX                          Integer32
    MAX-ACCESS                      read-only
    STATUS                          current
    DESCRIPTION  
        "Service flow traffic priority."
    ::= { icomWmacCmnServFlowEntry 9 }

icomWmacCmnServFlowMaxSustainedRate   OBJECT-TYPE 
    SYNTAX                            Unsigned32
    MAX-ACCESS                        read-only
    STATUS                            current
    DESCRIPTION  
        "Maximum sustained rate in bps."
    ::= { icomWmacCmnServFlowEntry 10 }

icomWmacCmnServFlowMinReservedRate   OBJECT-TYPE 
    SYNTAX                           Unsigned32
    MAX-ACCESS                       read-only
    STATUS                           current
    DESCRIPTION  
        "Minimum reserved rate in bps (all scheduling
        types but BE)."
    ::= { icomWmacCmnServFlowEntry 11 }

icomWmacCmnServFlowGrantInterval  OBJECT-TYPE 
    SYNTAX                        Unsigned32
    UNITS                         "micro-seconds"
    MAX-ACCESS                    read-only
    STATUS                        current
    DESCRIPTION  
        "Grant interval (UGS UL direction only)."
    ::= { icomWmacCmnServFlowEntry 12 }

icomWmacCmnServFlowSchedulingType   OBJECT-TYPE 
    SYNTAX                          INTEGER { bestEffort(1),
                                              nonRealTimePollingService(2),
                                              realTimePollingService(3),
                                              unsolicitedGrantService(4)
                                            }               
    MAX-ACCESS                      read-only
    STATUS                          current
    DESCRIPTION  
        "Scheduling type (BE, NRT-VR, RT-VR, UGS)."
    ::= { icomWmacCmnServFlowEntry 13 }

icomWmacCmnServFlowReqTxPolicy   OBJECT-TYPE 
    SYNTAX                       BITS { prePacking(0),
       				                    fragmentation(1),
                                        phs(2)
                                      } 
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "Transmission policy. This object specifies the status of
        the transmission policy bitmask. A '1' in a bit position 
        indicates that a specific attribute of the service flow 
        (i.e., pre-packing, fragmentation, PHS) is enabled. A 
        '0' in a bit position indicates that a specific attribute 
        of the service flow is disabled."
    ::= { icomWmacCmnServFlowEntry 14 }

icomWmacCmnServFlowWeight        OBJECT-TYPE
    SYNTAX                       Integer32
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "Proportional Fair Scheduling weight (in bytes) for BE, 
        NRT-VR, and RT-VR."
    ::= { icomWmacCmnServFlowEntry 15 }

icomWmacCmnServFlowEstablishedTime  OBJECT-TYPE 
    SYNTAX                          TimeStamp
    MAX-ACCESS                      read-only
    STATUS                          current
    DESCRIPTION  
        "Service flow Established Time."
    ::= { icomWmacCmnServFlowEntry 16 }

icomWmacCmnServFlowQueueType     OBJECT-TYPE 
    SYNTAX                       INTEGER { strict(1),
                                           mixed(2)
                                         }
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "Queue selection mode used by the dequeuing processing during transmission, 
        used to select between transmit queues 0 to 7. Two options are available. 
            Strict priority: Selection of queue is strict priority only, where 0 is highest. 
            Mixed priority: Selection of queue is strict priority for queues 0 to 3, followed 
                            by WFQ for queues 4 to 7 (DL only)."
    ::= { icomWmacCmnServFlowEntry 17 }

icomWmacCmnServFlowQueueWeight4    OBJECT-TYPE 
    SYNTAX                         Integer32
    MAX-ACCESS                     read-only
    STATUS                         current
    DESCRIPTION  
        "Used only when queue selection mode is mixed priority. Relative weight of 
        transmit queue 4."
    ::= { icomWmacCmnServFlowEntry 18 }

icomWmacCmnServFlowQueueWeight5  OBJECT-TYPE 
    SYNTAX                       Integer32
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION  
        "Used only when queue selection mode is mixed priority. Relative weight of 
        transmit queue 5."
    ::= { icomWmacCmnServFlowEntry 19 }

