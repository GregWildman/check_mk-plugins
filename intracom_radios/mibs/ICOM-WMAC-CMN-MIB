
ICOM-WMAC-CMN-MIB DEFINITIONS ::= BEGIN

-- =========================================================
-- ICOM WMAC COMMON MIB MODULE
-- =========================================================
                                             
IMPORTS                                         
   MODULE-IDENTITY, OBJECT-TYPE,                   
   Integer32, Counter64, Unsigned32, TimeTicks                                     
       FROM SNMPv2-SMI                   -- [RFC2578]
                                 
   MacAddress, TimeStamp, TruthValue, TEXTUAL-CONVENTION, TimeInterval
       FROM SNMPv2-TC                    -- [RFC2579] 
                                  
   MODULE-COMPLIANCE, OBJECT-GROUP
       FROM SNMPv2-CONF                  -- [RFC2580]
    
   InetAddressType, InetAddress
        FROM INET-ADDRESS-MIB            -- [RFC4001]

   InterfaceIndexOrZero
        FROM IF-MIB

   IcomDateTimeType
        FROM ICOM-PERFHIST-TC-MIB

   intracom                                        
       FROM ICOM-SMI;                            
   

icomWmacCmnMIB MODULE-IDENTITY
 
   LAST-UPDATED "201512220000Z"  -- dtzou: December 22, 2015
   ORGANIZATION "Intracom Wireless Group"
   CONTACT-INFO
   "   
       EMail: 
       Phone: 
       Co-editor:
       EMail:
       Phone:
   "

DESCRIPTION 
    "This MIB module defines common managed objects for the Base 
     and Terminal Station useful for the configuration and management
     of the Wireless MAC (WMAC)."

