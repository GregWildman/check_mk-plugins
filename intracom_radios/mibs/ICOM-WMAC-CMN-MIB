    icomWmacCmnPerfSf15MinIntervalUlErrPdu        Counter64,
    icomWmacCmnPerfSf15MinIntervalUlErrSdu        Counter64,
    icomWmacCmnPerfSf15MinIntervalUlErrSduFrag    Counter64,
    icomWmacCmnPerfSf15MinIntervalUlErrSduLength  Counter64
}

