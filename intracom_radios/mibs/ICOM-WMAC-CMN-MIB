        "
    ::= { icomWmacCmnPerfSfStats15MinIntervalEntry 8 }

-- -------------------------------------------------------------
-- icomWmacCmnPerfSfStats1DayIntervalTable
-- -------------------------------------------------------------

icomWmacCmnPerfSfStats1DayIntervalTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IcomWmacCmnPerfSfStats1DayIntervalEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "This table provides fine-grain service flow performance statistics
        for historical (completed) 1-day intervals.

        This table will have a maximum of 30 entries of completed 1-day
        intervals, to provide historical statistics for one month.
       "
    ::= { icomWmacCmnPerfObjects 9 }

icomWmacCmnPerfSfStats1DayIntervalEntry OBJECT-TYPE
    SYNTAX       IcomWmacCmnPerfSfStats1DayIntervalEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "An entry (conceptual row) in the
        icomWmacCmnPerfSfStats1DayIntervalTable.
       "
    INDEX  { icomWmacCmnComponentId,
             icomWmacCmnPeerId,
             icomWmacCmnSfId,
             icomWmacCmnPerfSfStats1DayIntervalNumber
           }
    ::= { icomWmacCmnPerfSfStats1DayIntervalTable 1 }

IcomWmacCmnPerfSfStats1DayIntervalEntry ::= SEQUENCE {
    icomWmacCmnPerfSfStats1DayIntervalNumber          Unsigned32,
    icomWmacCmnPerfSfStats1DayIntervalEndTime         IcomDateTimeType,
    icomWmacCmnPerfSfStats1DayIntervalElapsedTime     TimeInterval,
    icomWmacCmnPerfSfStats1DayIntervalSuspect         TruthValue,
    icomWmacCmnPerfSfStats1DayIntervalDlNumSdus       Counter64,
    icomWmacCmnPerfSfStats1DayIntervalUlNumSdus       Counter64,
    icomWmacCmnPerfSfStats1DayIntervalDlSduBytes      Counter64,
    icomWmacCmnPerfSfStats1DayIntervalUlSduBytes      Counter64
}

icomWmacCmnPerfSfStats1DayIntervalNumber OBJECT-TYPE
    SYNTAX       Unsigned32 (1..30)
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "A number that identifies the interval for which the set of
        statistics is available. The interval identified by 1 is the
        most recently completed 1-day interval, and the interval
        identified by N is the interval immediately preceding the one
        identified by N-1.
       "
    ::= { icomWmacCmnPerfSfStats1DayIntervalEntry 1 }

