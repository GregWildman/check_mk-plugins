--=========================================================
-- icomWmacCmnPerfSfStatsEnable
--=========================================================

icomWmacCmnPerfSfStatsEnable OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
       "This object enables/disables the collection of fine-grain
        historical performance statistics for the service flows with a
        1-minute granularity.

        If value is set to 'true(1)', then collection of historical
        performance statistics is enabled.

        If value is set to 'false(2)', then collection of historical
        performance statistics is disabled. Any existing historical
        performance statistics will be discarded.

        This object is persistent, and its value must be retained across
        reinitializations of the management system.
       "
    DEFVAL { true }
    ::= { icomWmacCmnPerfObjects 5 }

-- -------------------------------------------------------------
-- icomWmacCmnPerfSfStatsCurrentTable
-- -------------------------------------------------------------

icomWmacCmnPerfSfStatsCurrentTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF IcomWmacCmnPerfSfStatsCurrentEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
       "This table provides fine-grain service flow performance statistics
        for the current 1-minute interval. All table statistics are
        reset to zero after the completion of each 1-minute measurement
        interval.
       "
    ::= { icomWmacCmnPerfObjects 6 }

