   RemoteNodeTypeValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the value of connection mode of a connection."
		SYNTAX
			INTEGER {
				ef(1),		-- ef
				cn2100(2),	-- oa
				cn2300(3),	-- mxa
				cn2200(4),	-- mx
				cn4300(5),	-- xc
				xl(6),		-- xl
				unknown(7),
				cn4350(8),	-- l2x
				cn4200(9),	-- bam
                cn4200mc(10),  -- bam
				cn4200rs(11), -- UV 
                coredirector(12), -- coredirector
                coredirectorci(13) -- coredirectorCI
			}

	WavelengthValue	::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the wavelength of a port."
		SYNTAX
		OCTET STRING (SIZE(1..64))

	RedundancyModeValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the redundancy mode of a particular entity."
		SYNTAX
			INTEGER {
				unknown(1),
				not-present(2),
				primary(3),
				secondary(4),
				simplex(5)
			}

