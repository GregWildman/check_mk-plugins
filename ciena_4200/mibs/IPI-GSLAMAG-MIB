	    channel."
	::= { gslamAGChannelEntry 21 }

    gslamAGChannelExpOperSpecified   OBJECT-TYPE
	SYNTAX      OCTET STRING(SIZE(0..32))
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicate the expected operator specified value of this
	    GSLAM-AG channel."
	::= { gslamAGChannelEntry 22 }

    gslamAGChannelTrcSAPI   OBJECT-TYPE
	SYNTAX      OCTET STRING(SIZE(0..15))
