     	::= { gslamAGREL50ShelfEntry 21 }

     gslamAGREL50ShelfPrimarySyncMod1Index	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
		"Module index of the primary time sync
		source. When time sync is not provisioned
		or when the modules are in free run state,
		the value will be 0.This is for slot gr[1]"
	    ::= { gslamAGREL50ShelfEntry 22 }

	 gslamAGREL50ShelfSecondarySyncMod1Index	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
		"Module index of the secondary time sync source. 
		When time sync is not provisioned
		or when the modules are in free run state, the value will be 0.
		This is for slot gr[1]"
	    ::= { gslamAGREL50ShelfEntry 23 }

