	-- UV 5.0 CHANGE START

	-- IO module objects
	gslamAGREL50IOModuleTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGREL50IOModuleEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the IO modules of GSLAM-AG."
	    ::= { ipiGslamAGIOModuleObjs 2 }

	gslamAGREL50IOModuleEntry OBJECT-TYPE
		SYNTAX  		GslamAGREL50IOModuleEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in module table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGREL50ShelfIndex, gslamAGREL50IOModuleIndex }
	    ::= { gslamAGREL50IOModuleTable 1 }

	GslamAGREL50IOModuleEntry ::= SEQUENCE {
		gslamAGREL50IOModuleIndex			Unsigned32,
		gslamAGREL50IOModuleType			EquipmentTypeValue,
		gslamAGREL50IOModuleAction			ActionValue,
		gslamAGREL50IOModuleRowStatus		RowStatus,
		gslamAGREL50IOModuleLocation			LocationValue,
		gslamAGREL50IOModuleCLEI			CLEIValue,
		gslamAGREL50IOModulePrimaryState		PrimaryStateValue,
		gslamAGREL50IOModuleSecondaryState		SecondaryStateValue,
		gslamAGREL50IOModuleHWManufactureName	HWManufactureNameValue,
		gslamAGREL50IOModuleHWManufactureSerial	HWManufactureSerialValue,
		gslamAGREL50IOModuleHWRevision		HWRevisionValue,
		gslamAGREL50IOModuleHWManufacturePartName  HWPartNumberValue,
		gslamAGREL50IOModuleHWManufacturePartNumber HWPartNumberValue,
		gslamAGREL50IOModuleOperationalMode			ModuleOperationalModeValue,
		--gslamAGIOModuleAutoShutdown     FlagValue,
		gslamAGREL50IOROADMModuleOPLaserState		INTEGER,
		gslamAGREL50IOModuleDegreeId				Unsigned32,
		gslamAGREL50IOModuleGain					OCTET STRING,
		gslamAGREL50IOModulePump1ActualLBC		OCTET STRING,
		gslamAGREL50IOModulePump2ActualLBC		OCTET STRING,
		gslamAGREL50IOModulePump1KinkFreeCurrent	OCTET STRING,
		gslamAGREL50IOModulePump2KinkFreeCurrent	OCTET STRING,
		gslamAGREL50IOModuleOPSGroup1OPSMode		OPSModeValue,
		gslamAGREL50IOModuleOPSGroup1LOPMode		FlagValue,
		gslamAGREL50IOModuleOPSGroup2OPSMode		OPSModeValue,
		gslamAGREL50IOModuleOPSGroup2LOPMode		FlagValue,
		gslamAGREL50IOModuleFeatureCode			OCTET STRING,
		gslamAGREL50IOModuleFeatureKey			OCTET STRING,
		gslamAGREL50IOROADMModuleOCM1Degree    	Unsigned32,	
		gslamAGREL50IOROADMModuleOCM2Degree    	Unsigned32,	
		gslamAGREL50IOROADMModuleOCM3Degree    	Unsigned32,	
		gslamAGREL50IOROADMModuleOCM4Degree    	Unsigned32,
        gslamAGREL50IOROADMModuleDCFLossBorrowing   INTEGER,
        gslamAGREL50IOModuleFiberTiltCompensation   INTEGER,
        gslamAGREL50IOROADMModuleActualOPLaserState INTEGER,
        gslamAGREL50IOModuleDCMType                 CLEIValue, 
        gslamAGREL50IOModuleGainTCAThreshold      OCTET STRING,
        gslamAGREL50IOModuleLaserStatus           INTEGER,
        gslamAGREL50IOModulePump3ActualLBC        OCTET STRING,
        gslamAGREL50IOModulePump3KinkFreeCurrent  OCTET STRING,
        gslamAGREL50IOModule1425PumpPower         OCTET STRING,
        gslamAGREL50IOModule1455PumpPower         OCTET STRING,
        gslamAGREL50IOModuleTECCurrentPump1       OCTET STRING,
        gslamAGREL50IOModuleTECCurrentPump2       OCTET STRING,
        gslamAGREL50IOModuleTECCurrentPump3       OCTET STRING,
        gslamAGREL50IOModulePump1SubmountTemp     OCTET STRING,
        gslamAGREL50IOModulePump2SubmountTemp     OCTET STRING,
        gslamAGREL50IOModulePump3SubmountTemp     OCTET STRING
	}

	gslamAGREL50IOModuleIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index of GSLAM-AG IO module table. It uniquely identifies each module."
	    ::= { gslamAGREL50IOModuleEntry 1 }

	gslamAGREL50IOModuleType	OBJECT-TYPE
		SYNTAX EquipmentTypeValue
 	 	MAX-ACCESS  read-write
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the equipment type of this IO module."
 	 	::= { gslamAGREL50IOModuleEntry 2 }

