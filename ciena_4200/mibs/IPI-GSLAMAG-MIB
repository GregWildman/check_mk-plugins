		gslamAGSubChannelIfIndex                             Unsigned32,
		gslamAGSubChannelPrimaryState                        PrimaryStateValue,
		gslamAGSubChannelSecondaryState                      SecondaryStateValue,
		gslamAGSubChannelType                              EquipmentTypeValue 
	}

