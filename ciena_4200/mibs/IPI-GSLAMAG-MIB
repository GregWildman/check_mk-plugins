   	gslamAGREL50ConcOPVCxReceivedTrcOperSpecified   OBJECT-TYPE
		SYNTAX      OCTET STRING(SIZE(0..32))
		MAX-ACCESS  read-only
		STATUS      current
		DESCRIPTION
	    		"Indicate the received operator specified value of
	    		 this GSLAM-AG concatenated OPVCxNumic."
	::= { gslamAGREL50ConcOPVCxEntry 26 }

	gslamAGREL50ConcOPVCxExpTrc		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Indicates the expected trace value of the GSLAM-AG OPVCxNumic channel."
	::= { gslamAGREL50ConcOPVCxEntry 27 }

	gslamAGREL50ConcOPVCxTrc		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Indicates the transmitted trace value of the GSLAM-AG
			OPVCxNumic."
	::= { gslamAGREL50ConcOPVCxEntry 28 }

	gslamAGREL50ConcOPVCxReceivedTrc     OBJECT-TYPE
		SYNTAX      OCTET STRING(SIZE(1..64))
		MAX-ACCESS  read-only
		STATUS      current
		DESCRIPTION
	    		"Indicates the received trace value of the GSLAM-AG
	    		OPVCxNumic."
	::= { gslamAGREL50ConcOPVCxEntry 29 }

    gslamAGREL50ConcOPVCxCRSLpbkType     OBJECT-TYPE
    SYNTAX      INTEGER {
        unknown(1),
        not-present(2),
        operate(3),
        release(4)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the cross-connect loopback provisioning for OPVCX channel."
    ::= { gslamAGREL50ConcOPVCxEntry 30 }

    gslamAGREL50ConcOPVCxCRSLpbkDuration     OBJECT-TYPE
    SYNTAX  Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the duration of cross-connect loopback."
    ::= { gslamAGREL50ConcOPVCxEntry 31 }

	-- UV 5.0 CHANGE END



