		SYNTAX  		GslamAGREL50IOModuleVoaEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in module table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGREL50ShelfIndex, gslamAGREL50IOModuleIndex,
					gslamAGREL50IOModuleVoaIndex }
	    ::= { gslamAGREL50IOModuleVoaTable 1 }

	GslamAGREL50IOModuleVoaEntry ::= SEQUENCE {
		gslamAGREL50IOModuleVoaIndex			Unsigned32,
		gslamAGREL50IOModuleVoaRowStatus		RowStatus,
		gslamAGREL50IOModuleVoaAttenuation	OCTET STRING
	}

	gslamAGREL50IOModuleVoaIndex     OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Index of the VOA table. This maps right into the enum
					_IPIMgmtVmuxVoaType or _IPIMgmtDwrVoaType"
        ::= { gslamAGREL50IOModuleVoaEntry 1 }

	gslamAGREL50IOModuleVoaRowStatus     OBJECT-TYPE
            SYNTAX      RowStatus
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Rowstatus value for this table"
        ::= { gslamAGREL50IOModuleVoaEntry 2 }

	gslamAGREL50IOModuleVoaAttenuation	OBJECT-TYPE
		SYNTAX      OCTET STRING(SIZE(1..20))
		MAX-ACCESS  read-write
		STATUS      current
		DESCRIPTION
			"Indicates the VOA attenuation val for a DWR or VMUX module"
		::= {gslamAGREL50IOModuleVoaEntry 3 }


	-- UV 5.0 CHANGE END

