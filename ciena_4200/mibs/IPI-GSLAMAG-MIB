	-- port objects
	gslamAGPortTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGPortEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the port info of GSLAM-AG."
	    ::= { ipiGslamAGPortObjs 1 }

	gslamAGPortEntry OBJECT-TYPE
		SYNTAX  		GslamAGPortEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in port table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGShelfIndex, gslamAGIOModuleIndex, gslamAGPortIndex }
	    ::= { gslamAGPortTable 1 }


	GslamAGPortEntry ::= SEQUENCE {
		gslamAGPortIndex		    Unsigned32,
		gslamAGPortIfIndex		    Unsigned32,
		gslamAGPortType			    EquipmentTypeValue,
		gslamAGPortAction		    ActionValue,
		gslamAGPortRowStatus		RowStatus,
		gslamAGPortCLEI			    CLEIValue,
		gslamAGPortPrimaryState		PrimaryStateValue,
		gslamAGPortSecondaryState	SecondaryStateValue,
		gslamAGPortWavelength		WavelengthValue, -- FREQ
		gslamAGPortProtFunction		ProtectionEntityValue, -- PROTN
		gslamAGPortClearDelay		Unsigned32,	-- CLRDLY
		gslamAGPortDropPacket		FlagValue,	-- DROPPKT
		gslamAGPortECC			    FlagValue,	-- ECC
		gslamAGPortIfDirection		IfDirValue,  	-- IF
		gslamAGPortLossBudget		TcvrReachValue,	-- LOSSB
		gslamAGPortNDP			    FlagValue,	-- NDP
		gslamAGPortSFThreshold		Unsigned32,	-- SFTH
		gslamAGPortSDThreshold		Unsigned32,	-- SDTH
		gslamAGPortSESThreshold		Unsigned32,	-- SESTH
		gslamAGPortTxOptPower		OCTET STRING,
		gslamAGPortRxOptPower		OCTET STRING,
		gslamAGPortTxOptMonLev  	OCTET STRING,	-- OPRMONLEV
		gslamAGPortRxOptMonLev  	OCTET STRING,   -- OPTMONLEV
		gslamAGPortProtocolCap  	ProtocolCapValue, -- PROTCAP
		gslamAGPortClassCap     	ClassCapValue,	-- CLASSCAP
		gslamAGPortDataFieldSize    Unsigned32,	-- DATAFLDSZ
		gslamAGPortSESSThreshold    Unsigned32,	-- SESSTH
		gslamAGPortCSESThreshold    Unsigned32,	-- CSESTH
		gslamAGPortFEC		      	FECValue,  -- FEC
		gslamAGPortMedium       	MediumValue,	-- MEDIUM
		gslamAGPortLineCoding   	LineCodingValue, -- LINECDE
		gslamAGPortTiming       	TimingValue,	-- TMG
		gslamAGPortDegMonLev    	Unsigned32,	-- DEGM
		gslamAGPortDegThreshold 	Integer32,	-- DEGTHR
		gslamAGPortGCC0     		FlagValue,	-- GCC0
		gslamAGPortTIMDetectMode    TIMModeValue,	-- TIMDETM
		gslamAGPortTIMActDisable    FlagValue,	-- TIMACTDIS
		gslamAGPortExpSAPI      	OCTET STRING,	-- EXPSAPI
		gslamAGPortExpDAPI      	OCTET STRING,   -- EXPDAPI
		gslamAGPortExpOperSpecified  OCTET STRING,   -- EXPOPR
		gslamAGPortTrcSAPI      	OCTET STRING,	-- TRCSAPI
		gslamAGPortTrcDAPI      	OCTET STRING,	-- TRCDAPI
		gslamAGPortTrcOperSpecified  OCTET STRING,   -- TRCOPR
		gslamAGPortReceivedTrcSAPI	OCTET STRING,
		gslamAGPortReceivedTrcDAPI  OCTET STRING,
		gslamAGPortReceivedTrcOperSpecified  OCTET STRING,
		gslamAGPortDirection    	INTEGER,	-- PDIRN
		gslamAGPortHWManufactureName    HWManufactureNameValue,
		gslamAGPortHWManufactureSerial  HWManufactureSerialValue,
		gslamAGPortHWRevision       	HWRevisionValue,
		gslamAGPortHWManufacturePartName	HWPartNumberValue,
		gslamAGPortHWManufacturePartNumber      HWPartNumberValue,
		gslamAGPortMuxStructure     INTEGER,
		gslamAGPortCompatability	IfDirValue,
		gslamAGPortAutoNegotiation	PortAutoNegotiationValue,
		gslamAGPortCableType		EEPortCableTypeValue,
		gslamAGPortLoopbackDuration	Unsigned32,
		gslamAGPortLaserMode		LaserModeValue,
		gslamAGPortSignalMapping	SignalMappingValue,
		gslamAGPortLOSLevel		OCTET STRING,
		gslamAGPortOSCTxPower  		OCTET STRING,   -- osc-txpower
		gslamAGPortOSCRxPower  		OCTET STRING,   -- osc-rxpower
		gslamAGPortOSCTxOptMonLev  	OCTET STRING,	-- osc-optmonlev
		gslamAGPortOSCRxOptMonLev  	OCTET STRING,   -- osc-oprmonlev
		gslamAGPortTxSpanLoss  		OCTET STRING,   -- tx span loss
		gslamAGPortRxSpanLoss  		OCTET STRING,   -- rx span loss
		gslamAGPortOSLRxMonLev  	OCTET STRING,   -- oslrmon-lev
		gslamAGPortOSLTxMonLev  	OCTET STRING,   -- osltmon-lev
		gslamAGPortActualLBC  		OCTET STRING,   -- actual-lbc
		gslamAGPortLBCMonLev  		OCTET STRING,   -- lbc-monlev
		gslamAGPortAttenuation		OCTET STRING,
		gslamAGPortMaxFrameSize		Unsigned32,
		gslamAGPortSpanLoss		FlagValue,
		gslamAGPortFreqChannel		FreqChannelNumValue,
		gslamAGPortLabel		OCTET STRING,
		gslamAGPortVideoFrameType	INTEGER,
		gslamAGPortSpeed		INTEGER,
		gslamAGPortGfpOutGoingUPI	OCTET STRING,
		gslamAGPortGfpIncomingUPI	OCTET STRING,
		gslamAGPortGfpExpectedUPI	OCTET STRING,
		gslamAGPortLOSClearLevel	OCTET STRING,
		gslamAGPortOOSLaserMode		LaserModeValue,
		gslamAGPortAISLAction		FlagValue,
		gslamAGPortPCSCoding		PCSCoding,
		gslamAGPortChanSize		   ChanSizeValue,
		gslamAGPortOperSpeed		INTEGER,
		gslamAGPortDuplexMode		DuplexMode,
        gslamAGPortLpbkType         INTEGER,
                gslamAGPortLOSThreshold         OCTET STRING,
		gslamAGPortAddress		OCTET STRING,
		gslamAGPortChannelType		INTEGER,	
		gslamAGPortFlowControl		FlagValue,
		gslamAGPortLaserXparency	FlagValue, 
		gslamAGPortLoopTiming		FlagValue,
		gslamAGPortSwitchNotification	INTEGER,
		gslamAGPortSwitchDuration	INTEGER,
		gslamAGPortLinkInitMode 	INTEGER,
		gslamAGPortNegotiationTimer	INTEGER,
		gslamAGPortLinkFailureTimer	INTEGER,
		gslamAGPortSpeedNegotiation	INTEGER,
		gslamAGPortCrdtMode		INTEGER,
		gslamAGPortCrdtValue		INTEGER,
		gslamAGPortCrdtRecoveryTimer	INTEGER,
		gslamAGPortFaultNotification	INTEGER,
		gslamAGPortFaultHoldOffDuration	INTEGER,
		gslamAGPortTimingMode		INTEGER,
		gslamAGPortECCRate		INTEGER,
		gslamAGPortAssociate		INTEGER,
		gslamAGPortRemoteVfptId		INTEGER,
		gslamAGPortMaxUtilization	INTEGER,
		gslamAGPortMinIPG		Unsigned32,
		gslamAGPortLocalCondition	OCTET STRING,
		gslamAGPortRemoteCondition	OCTET STRING,
		gslamAGPortOspfAdminStatus	INTEGER,
		gslamAGPortOspfProfileId	Unsigned32,
		gslamAGPortOspfProfileName	OCTET STRING,
		gslamAGPortOspfCost		Integer32
	}

	gslamAGPortIndex	OBJECT-TYPE
