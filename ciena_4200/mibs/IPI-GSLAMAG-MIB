	-- Control connection table
	gslamAGControlConnectionTable    OBJECT-TYPE
		SYNTAX          SEQUENCE OF GslamAGControlConnectionEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION
			"A table of status of control connections to the neighboring nodes."
		::= { ipiGslamAGControlConnectionObjs 1 }

	gslamAGControlConnectionEntry    OBJECT-TYPE
		SYNTAX          GslamAGControlConnectionEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION
			"A row in the CN4200's control connection table."
		INDEX  { gslamAGControlConnectionEthIndex }
		::= { gslamAGControlConnectionTable 1 }


	GslamAGControlConnectionEntry ::= SEQUENCE {
		gslamAGControlConnectionEthIndex         Unsigned32,
		gslamAGControlConnectionLocalIP          IpAddress,
		gslamAGControlConnectionRemoteNodeIP     IpAddress,
		gslamAGControlConnectionRemoteIP         IpAddress,
		gslamAGControlConnectionRemoteNodeType	RemoteNodeTypeValue,
		gslamAGControlConnectionLocalNodeTID	OCTET STRING,
		gslamAGControlConnectionRemoteNodeTID	OCTET STRING,
		gslamAGControlConnectionStatus		INTEGER,
		gslamAGControlConnectionType		INTEGER,
		gslamAGControlConnectionRemoteEthIndex         Unsigned32
	}

	gslamAGControlConnectionEthIndex                  OBJECT-TYPE
		SYNTAX          Unsigned32
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION
			"Indicates the index of the management ethernet interface."
		::= { gslamAGControlConnectionEntry 1 }

	gslamAGControlConnectionLocalIP                  OBJECT-TYPE
		SYNTAX          IpAddress
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION
			"Indicates the IP address of the local end of this control connection."
		::= { gslamAGControlConnectionEntry 2 }

	gslamAGControlConnectionRemoteNodeIP         OBJECT-TYPE
		SYNTAX          IpAddress
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION
                        "Indicates the IP address of the remote node of this control connection."
		::= { gslamAGControlConnectionEntry 3 }

	gslamAGControlConnectionRemoteIP         OBJECT-TYPE
		SYNTAX          IpAddress
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION
                        "Indicates the IP address of the remote end of this control connection."
		::= { gslamAGControlConnectionEntry 4 }

   	gslamAGControlConnectionRemoteNodeType		OBJECT-TYPE
        	SYNTAX	RemoteNodeTypeValue
        	MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION
                        "Indicates the equiptment type of the remote end of this control connection."
		::= { gslamAGControlConnectionEntry 5 }

	gslamAGControlConnectionLocalNodeTID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(1..20))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the TID of the local end of this control connection."
	::= { gslamAGControlConnectionEntry 6 }

	gslamAGControlConnectionRemoteNodeTID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(1..20))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the TID of the remote end of this control connection."
	::= { gslamAGControlConnectionEntry 7 }

	gslamAGControlConnectionStatus	OBJECT-TYPE
		SYNTAX		INTEGER {
						unknown(1),
						ndp-enabled(2),
						admin-enabled(3)
		}
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate if the connection is ndp or admin enabled."
	::= { gslamAGControlConnectionEntry 8 }

	gslamAGControlConnectionType	OBJECT-TYPE
		SYNTAX		INTEGER {
						unknown(1),
						pt2pt(2),
						backplane(3)
		}
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate if the connection is point-to-point or backplane."
	::= { gslamAGControlConnectionEntry 9 }

	gslamAGControlConnectionRemoteEthIndex                  OBJECT-TYPE
		SYNTAX          Unsigned32
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION
			"Indicates the remote index of the management ethernet interface."
		::= { gslamAGControlConnectionEntry 10 }

   -- GSLAM-AG PE association objects.
	gslamAGPEAssociationTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGPEAssociationEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the all the PE association of GSLAM-AG."
	    ::= { ipiGslamAGPEAssociationObjs 1 }

	gslamAGPEAssociationEntry OBJECT-TYPE
		SYNTAX  		GslamAGPEAssociationEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG PE association table."
		INDEX   { gslamAGPEAssociationFromIfIndex, gslamAGPEAssociationToIfIndex }
	    ::= { gslamAGPEAssociationTable 1 }

	GslamAGPEAssociationEntry ::= SEQUENCE {
		gslamAGPEAssociationFromIfIndex	Unsigned32,
		gslamAGPEAssociationToIfIndex	Unsigned32,
		gslamAGPEAssociationFromPEType	EquipmentTypeValue,
		gslamAGPEAssociationToPEType	EquipmentTypeValue,
		gslamAGPEAssociationDirection	ConnModeValue,
		gslamAGPEAssociationCktID		OCTET STRING
	}

	gslamAGPEAssociationFromIfIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The first index of GSLAM-AG PE association table. It indicates
			the 'from' part of ifIndex in the PE association. Each ifIndex is a 32-bit
			unsigned integer. From left (most significant) to right, we allocate 4
			bits for unit(shelf) id, 4 bits for IO module id, 4 bits for interface(port)
			id, 4 bits for channel id, and the rest (rightmost) 16 bits for OPVC group."
	    ::= { gslamAGPEAssociationEntry 1 }

	gslamAGPEAssociationToIfIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The second index of GSLAM-AG PE association table. It indicates
			the 'to' part of ifIndex in the PE association. Each ifIndex is a 32-bit
			unsigned integer. From left (most significant) to right, we allocate 4
			bits for unit(shelf) id, 4 bits for IO module id, 4 bits for interface(port)
			id, 4 bits for channel id, and the rest (rightmost) 16 bits for OPVC group."
	    ::= { gslamAGPEAssociationEntry 2 }

	gslamAGPEAssociationFromPEType	OBJECT-TYPE
		SYNTAX		EquipmentTypeValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"The PE type of the 'from' part in PE association"
	    ::= { gslamAGPEAssociationEntry 3}

	gslamAGPEAssociationToPEType	OBJECT-TYPE
		SYNTAX		EquipmentTypeValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"The PE type of the 'to' part in PE association"
	    ::= { gslamAGPEAssociationEntry 4}

	gslamAGPEAssociationDirection	OBJECT-TYPE
		SYNTAX	ConnModeValue
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the direction of this PE association. For unidirection, traffic goes
				from 'from' to 'to'. Default value is bidirection"
 	 	::= { gslamAGPEAssociationEntry 5}

	gslamAGPEAssociationCktID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"The connection ID management application using for creating this connection, 
			Return empty string if not available."
	    ::= { gslamAGPEAssociationEntry 6}

