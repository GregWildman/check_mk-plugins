 	 	::= { gslamAGIOModuleEntry 26 }

	gslamAGIOModuleFeatureKey     OBJECT-TYPE
       		SYNTAX      OCTET STRING(SIZE(0..40))
       		MAX-ACCESS  read-write
       		STATUS      current
       		DESCRIPTION
				"A key to enable specific set of services in CN4200 shelf.
				This is write only parameter at module level"
 	 	::= { gslamAGIOModuleEntry 27 }

    gslamAGIOModuleDCFLossBorrowing     OBJECT-TYPE
        SYNTAX  INTEGER {
             not-applicable(1),
             enabled(2),
             disabled(3)
        }
        MAX-ACCESS      read-write
        STATUS    current
        DESCRIPTION
            "Indicates if module loss borrowing is enabled or disabled. 
             Default Value is DISABLED for OAV-VS-U-CP"
        ::= { gslamAGIOModuleEntry 28 }

