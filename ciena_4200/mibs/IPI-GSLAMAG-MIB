	-- ********** PDU TABLE START *************
	gslamAGPduTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGPduEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the PDU info of GSLAM-AG."
	    ::= { ipiGslamAGPduObjs 1 }

	gslamAGPduEntry OBJECT-TYPE
		SYNTAX  		GslamAGPduEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in pdu table.  Entries can't be deleted 
			 or created via SNMP ops."
		INDEX   { gslamAGPduIndex }
	    ::= { gslamAGPduTable 1 }

	GslamAGPduEntry ::= SEQUENCE {
		gslamAGPduIndex		Unsigned32,
		gslamAGPduPort1		Unsigned32,
		gslamAGPduPort2		Unsigned32,
		gslamAGPduPort3 	Unsigned32,
		gslamAGPduState		INTEGER
	}

	gslamAGPduIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
		"Index of GSLAM-AG Pdu table. It uniquely identifies each pdu."
	    ::= { gslamAGPduEntry 1 }

	gslamAGPduPort1	OBJECT-TYPE
		SYNTAX 		Unsigned32
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
		"Indicates the shelfid [1-57] power feed 1 is supplying power to."
 	 	::= { gslamAGPduEntry 2 }

	gslamAGPduPort2	OBJECT-TYPE
		SYNTAX 		Unsigned32
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
		"Indicates the shelfid [1-57] power feed 2 is supplying power to."
 	 	::= { gslamAGPduEntry 3 }

	gslamAGPduPort3	OBJECT-TYPE
		SYNTAX 		Unsigned32
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
		"Indicates the shelfid [1-57] power feed 3 is supplying power to."
 	 	::= { gslamAGPduEntry 4 }

	gslamAGPduState	OBJECT-TYPE
		SYNTAX      INTEGER {
			unknown (0), 
			is-nr (1),
			oos-au (2)
		}
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
		"Indicates the state of the pdu"
 	 	::= { gslamAGPduEntry 5 }

	-- ********** PDU TABLE END *************

