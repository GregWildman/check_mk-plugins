	-- ************** ESOM SUPPORT STARTS **************
		gslamAGREL50ChannelLocalExtn           OBJECT-TYPE
			SYNTAX          INTEGER 
									{
									not-applicable(0),
									zero(1),
									ten(2),
									twenty(3),
									thirty(4),
									forty(5),
									fifty(6),
									sixty(7),
									seventy(8),
									eighty(9),
									ninety(10),
									hundred(11)
									}	
			MAX-ACCESS      read-write
			STATUS          current
			DESCRIPTION
				"Indicates the maximum allowed distance b/w CN4200 client port and directly attached equipment"
			::= { gslamAGREL50ChannelEntry 42 }
	
		gslamAGREL50ChannelNwExtn           OBJECT-TYPE
			SYNTAX          INTEGER {
						not-applicable(1),
						normal(2),
						maximum(3)
						}
			MAX-ACCESS      read-write
			STATUS          current
			DESCRIPTION
				"Indicates the network distance extension mode"
			::= { gslamAGREL50ChannelEntry 43 }
	
		gslamAGREL50ChannelCompression           OBJECT-TYPE
				SYNTAX        	FlagValue  
				MAX-ACCESS      read-write
				STATUS          current
				DESCRIPTION
					"Provision the compression datapath option against the CAG channel"
			::= { gslamAGREL50ChannelEntry 44 }
	
		gslamAGREL50ChannelNDP		OBJECT-TYPE
			SYNTAX        	FlagValue 
			MAX-ACCESS      read-write
			STATUS          current
			DESCRIPTION
				"Indicate the state of NDP on the specific network optical interface"
			::= { gslamAGREL50ChannelEntry 45 }
	
	
		gslamAGREL50ChannelOSPF		OBJECT-TYPE
			SYNTAX        	FlagValue 
			MAX-ACCESS      read-write
			STATUS          current
			DESCRIPTION
				"Indicate the state of the OSPF Protocol for the interface port"
			::= { gslamAGREL50ChannelEntry 46 }
	
		gslamAGREL50ChannelGroupSize           OBJECT-TYPE
				SYNTAX          INTEGER
						{
						not-applicable(1),
						sts48c(2),
						sts12c(3),
						sts3c(5),
						vc4-16c(6),
						vc4-4c(7),
						vc4(8)
						}
				MAX-ACCESS      read-write
				STATUS          current
				DESCRIPTION
					"Indicates the number of concatenated sts-1/vc4, default is zero"
			::= { gslamAGREL50ChannelEntry 47 }
	
