    	SYNTAX  	OCTET STRING(SIZE(0..15))
    	MAX-ACCESS  read-write
   	 	STATUS      current
    	DESCRIPTION
     		"The actual rx optical power of the optical channel.
			 The default value would be N/A."
   	::= {gslamAGOCCChannelEntry 36}

    gslamAGOCCChannelPreEmphasisOffset    OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..15))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The pre-emphasis offset of the optical channel."
    ::= {gslamAGOCCChannelEntry 37}

	-- concatenated OPVCx objects
	gslamAGConcOPVCxTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGConcOPVCxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the concatenated OPVCx info of GSLAM-AG."
	    ::= { ipiGslamAGConcOPVCxObjs 1 }

	gslamAGConcOPVCxEntry OBJECT-TYPE
		SYNTAX  		GslamAGConcOPVCxEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in concatenated OPVCx table."
		INDEX   { gslamAGConcOPVCxIndex }
	    ::= { gslamAGConcOPVCxTable 1 }


	GslamAGConcOPVCxEntry ::= SEQUENCE {
		gslamAGConcOPVCxIndex		Unsigned32,
		gslamAGConcOPVCxIfIndex	Unsigned32,
		gslamAGConcOPVCxType		EquipmentTypeValue,
		gslamAGConcOPVCxAction		ActionValue,
		gslamAGConcOPVCxPrimaryState	PrimaryStateValue,
		gslamAGConcOPVCxSecondaryState	SecondaryStateValue,
		gslamAGConcOPVCxProtFunction	ProtectionEntityValue,
		gslamAGConcOPVCxSESThreshold	Unsigned32,     -- SESTH
		gslamAGConcOPVCxCSESThreshold Unsigned32, 	-- CSESTH
		gslamAGConcOPVCxDegMonLev 	Unsigned32,     -- DEGM
		gslamAGConcOPVCxDegThreshold  Integer32,	-- DEGTHR
		gslamAGConcOPVCxOCIProtSwitch FlagValue,	    -- OCIPS
		gslamAGConcOPVCxTIMDetectMode TIMModeValue,   -- TIMDETM
		gslamAGConcOPVCxTIMActDisable FlagValue,	-- TIMACTDIS
		gslamAGConcOPVCxExpSAPI   	OCTET STRING,	-- EXPSAPI
		gslamAGConcOPVCxExpDAPI   	OCTET STRING,   -- EXPDAPI
		gslamAGConcOPVCxExpOperSpecified   OCTET STRING,   -- EXPOPR
		gslamAGConcOPVCxTrcSAPI   	OCTET STRING,	-- TRCSAPI
		gslamAGConcOPVCxTrcDAPI   	OCTET STRING,   -- TRCDAPI
		gslamAGConcOPVCxTrcOperSpecified   OCTET STRING,   -- TRCOPR
		gslamAGConcOPVCxReceivedTrcSAPI   OCTET STRING,
		gslamAGConcOPVCxReceivedTrcDAPI   OCTET STRING,
		gslamAGConcOPVCxReceivedTrcOperSpecified   OCTET STRING,
		gslamAGConcOPVCxExpTrc		OCTET STRING,   -- EXPTRC
		gslamAGConcOPVCxTrc		    OCTET STRING,   -- TRC
		gslamAGConcOPVCxReceivedTrc	OCTET STRING,	-- INCTRC
        gslamAGConcOPVCxCRSLpbkType       INTEGER,
        gslamAGConcOPVCxCRSLpbkDuration   Unsigned32 

		-- gslamAGConcOPVCxRowStatus	RowStatusValue
		-- gslamAGConcOPVCMasterTimeSlot Unsigned32
	}

	gslamAGConcOPVCxIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index of GSLAM-AG concatenated OPVCx table. It uniquely identifies
			each concatenated OPVCx."
