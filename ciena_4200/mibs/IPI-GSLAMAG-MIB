	gslamAGTopologyTable	OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGTopologyEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the current status of
			 all optical topologies in the GSLAM-AG."
	    ::= { ipiGslamAGTopologyObjs 1 }

	gslamAGTopologyEntry OBJECT-TYPE
		SYNTAX  		GslamAGTopologyEntry
		MAX-ACCESS		not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG Optical Topology table."
		INDEX { gslamAGTopologyShelf,gslamAGTopologyModule,
                gslamAGTopologyPort, gslamAGTopologyChnl}
	    ::= { gslamAGTopologyTable 1 }

	GslamAGTopologyEntry ::= SEQUENCE {
        gslamAGTopologyShelf        Unsigned32,
        gslamAGTopologyModule       Unsigned32,
        gslamAGTopologyPort     Unsigned32,
        gslamAGTopologyChnl     Unsigned32,
		gslamAGTopologyDir			INTEGER,
		gslamAGTopologyRX        		OCTET STRING,
		gslamAGTopologyTX        		OCTET STRING,
		gslamAGTopologyProtectionInfo		INTEGER,
		gslamAGTopologyRXProtection   	OCTET STRING,
		gslamAGTopologyTXProtection   	OCTET STRING
	}

    gslamAGTopologyShelf OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
            "The src shelf aid"
            ::= { gslamAGTopologyEntry 1 }

    gslamAGTopologyModule OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
            "The src module aid"
        ::= { gslamAGTopologyEntry 2 }

    gslamAGTopologyPort OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The src port aid"
        ::= { gslamAGTopologyEntry 3 }

    gslamAGTopologyChnl OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The src chnl aid"
        ::= { gslamAGTopologyEntry 4 }

	gslamAGTopologyDir OBJECT-TYPE
		SYNTAX  INTEGER {
            unknown(1),
            not-applicable(2),
            none(3),
            rx(4),
            tx(5),
            rx-tx(6)
        }
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The direction aid"
       	::= { gslamAGTopologyEntry 5 }

	gslamAGTopologyRX OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The RX side topology chain. It contains the following
				(inporttype-portIfIndex,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-portIfIndex)+|
				where portype is a numeric val,
					  portIfIndex is a numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a RX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-Y|X-Y,ModType,0,0,0,0,X-Y'|..."
       	::= { gslamAGTopologyEntry 6 }

	gslamAGTopologyTX OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The TX side topology chain. It contains the following
				(inporttype-portIfIndex,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-portIfIndex)+|
				where portype is a numeric val,
					  portIfIndex is a numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a TX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-Y|X-Y,ModType,0,0,0,0,0,X-Y'|..."
       	::= { gslamAGTopologyEntry 7 }

	gslamAGTopologyProtectionInfo OBJECT-TYPE
		SYNTAX  INTEGER {
			unknown(0),
            no(1),
            yes(2)
        }
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"This provides info to NE if protection data is to be 
			 provided (yes) or not. Working data is always provided. "
       	::= { gslamAGTopologyEntry 8 }

	gslamAGTopologyRXProtection OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The RX side protection topology chain. It contains the following
				(inporttype-portIfIndex ,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-portIfIndex )+|
				where portype is a numeric val,
					  portIfIndex is numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a RX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-Y|X-Y,ModType,0,0,0,0,X-Y'|..."
       	::= { gslamAGTopologyEntry 9 }

	gslamAGTopologyTXProtection OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The TX side topology chain. It contains the following
				(inporttype-portIfIndex ,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-portIfIndex )+|
				where portype is a numeric val,
					  portIfIndex is numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a TX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-Y|X-Y,ModType,0,0,0,0,0,X-Y'|..."
       	::= { gslamAGTopologyEntry 10}

	
