	gslamAGREL50FabricCXEntry OBJECT-TYPE
		SYNTAX  		GslamAGREL50FabricCXEntry
		MAX-ACCESS		not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG fabric ConnectionNumic table."
		INDEX   { 	gslamAGREL50FabricCXFromSIndex,gslamAGREL50FabricCXFromMIndex,
			  		gslamAGREL50FabricCXFromPIndex,gslamAGREL50FabricCXFromCIndex,
			  		gslamAGREL50FabricCXFromTIndex,gslamAGREL50FabricCXToSIndex,
			  		gslamAGREL50FabricCXToMIndex,gslamAGREL50FabricCXToPIndex,
			  		gslamAGREL50FabricCXToCIndex,gslamAGREL50FabricCXToTIndex}
	    ::= { gslamAGREL50FabricCXTable 1 }

	GslamAGREL50FabricCXEntry ::= SEQUENCE {
		gslamAGREL50FabricCXFromSIndex      Unsigned32,
       	gslamAGREL50FabricCXFromMIndex      Unsigned32,
        gslamAGREL50FabricCXFromPIndex      Unsigned32,
        gslamAGREL50FabricCXFromCIndex      Unsigned32,
        gslamAGREL50FabricCXFromTIndex      Unsigned32,
		gslamAGREL50FabricCXToSIndex        Unsigned32,
        gslamAGREL50FabricCXToMIndex        Unsigned32,
        gslamAGREL50FabricCXToPIndex        Unsigned32,
        gslamAGREL50FabricCXToCIndex        Unsigned32,
        gslamAGREL50FabricCXToTIndex        Unsigned32,
		gslamAGREL50FabricCXRowStatus       RowStatus,
		gslamAGREL50FabricCXDirection	    ConnModeValue,
		gslamAGREL50FabricCXCktID		    OCTET STRING,
		gslamAGREL50FabricCXLPID		    OCTET STRING
	}

	gslamAGREL50FabricCXFromSIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index1 of GSLAM-AG GslamAGREL50FabricCXEntry table. 
			 It indicates shelf index of
			 the 'from' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 1 }

	gslamAGREL50FabricCXFromMIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index2 of GSLAM-AG fabric ConnectionNumic table. It indicates module index of
			the 'from' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 2 }

	gslamAGREL50FabricCXFromPIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index3 of GSLAM-AG fabric ConnectionNumic table. It indicates port index of
			the 'from' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 3 }

	gslamAGREL50FabricCXFromCIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index4 of GSLAM-AG fabric ConnectionNumic table. It indicates chnl index of
			the 'from' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 4 }

	gslamAGREL50FabricCXFromTIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index5 of GSLAM-AG fabric ConnectionNumic table. 
			It indicates ts index of
			the 'from' part of ifIndex in the fabric ConnectionNumic.
			It is a bit-encoded entity and '1' represents that TS is present
			and '0' represents not present."
	    ::= { gslamAGREL50FabricCXEntry 5 }

	gslamAGREL50FabricCXToSIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index1 of GSLAM-AG fabric ConnectionNumic table. It indicates shelf index of
			the 'to' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 6 }

	gslamAGREL50FabricCXToMIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index2 of GSLAM-AG fabric ConnectionNumic table. It indicates Module index of
			the 'to' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 7 }

	gslamAGREL50FabricCXToPIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index3 of GSLAM-AG fabric ConnectionNumic table. It indicates port index of
			the 'to' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 8 }

	gslamAGREL50FabricCXToCIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index4 of GSLAM-AG fabric ConnectionNumic table. It indicates chnl index of
			the 'to' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50FabricCXEntry 9 }

	gslamAGREL50FabricCXToTIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index5 of GSLAM-AG fabric ConnectionNumic table. 
			It indicates ts index of
			the 'to' part of ifIndex in the fabric ConnectionNumic.
			It is a bit-encoded entity and '1' represents that TS is present
            and '0' represents not present."
	    ::= { gslamAGREL50FabricCXEntry 10 }

	gslamAGREL50FabricCXRowStatus			OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"Current status of this GSLAM-AG fabric ConnectionNumic entry."
	    ::= { gslamAGREL50FabricCXEntry 11 }

	gslamAGREL50FabricCXDirection			OBJECT-TYPE
		SYNTAX	ConnModeValue
 	 	MAX-ACCESS  read-write
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the direction of this fabric ConnectionNumic. For unidirection, traffic goes
				from 'from' to 'to'. Default value is bidirection"
 	 	::= { gslamAGREL50FabricCXEntry 12 }

	gslamAGREL50FabricCXCktID				OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The ConnectionNumic ID management application using for creating this ConnectionNumic, 
