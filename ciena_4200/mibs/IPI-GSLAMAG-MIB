	gslamAGTimeSlotTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGTimeSlotEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the time slot info of GSLAM-AG."
	    ::= { ipiGslamAGTimeSlotObjs 1 }

	gslamAGTimeSlotEntry OBJECT-TYPE
		SYNTAX  		GslamAGTimeSlotEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in TimeSlot table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGShelfIndex, gslamAGIOModuleIndex, gslamAGPortIndex,
			gslamAGChannelIndex, gslamAGTimeSlotIndex  }
	    ::= { gslamAGTimeSlotTable 1 }


	GslamAGTimeSlotEntry ::= SEQUENCE {
		gslamAGTimeSlotIndex		Unsigned32,
		gslamAGConcOPVCxId		Unsigned32,
		gslamAGTimeSlotAvailability             INTEGER

	}

	gslamAGTimeSlotIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"It uniquely identifies each TimeSlot in a particular channel."
    	::= { gslamAGTimeSlotEntry 1 }

	gslamAGConcOPVCxId	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicate the index of the OPVCX table which is using this timeslot.
			Returns 0 if it does not belong to any OPVCX group."
	    ::= { gslamAGTimeSlotEntry 2 }

	gslamAGTimeSlotAvailability	OBJECT-TYPE
		SYNTAX		INTEGER { unknown(1),available(2),used(3) }
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Flag to indicate if the timeslot is used during provisioning."
	    ::= { gslamAGTimeSlotEntry 3 }

