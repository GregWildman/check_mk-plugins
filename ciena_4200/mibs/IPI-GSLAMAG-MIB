        SYNTAX      INTEGER {
            not-applicable(1),
            enabled (2),
            disabled (3)
        }

        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Fiber Tilt compensation provisioning in stand-alone classic shelf .
             Default Value depends on module type."
        ::= { gslamAGIOModuleEntry 29}

    gslamAGIOModuleOPLaserState  OBJECT-TYPE
        SYNTAX      INTEGER {
            laser-state-not-present (0), --unsupport fiber type 
            laser-state-auto (1),
            laser-state-off (2),
            laser-state-on (3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates laser mode of Optical Amplifier upon detection
             of input signal loss. This is replacing the autoshutdown flag"
