	-- CondAll objs
	gslamAGCondAllTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGCondAllEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the current status of all standing conditions in the GSLAM-AG."
	    ::= { ipiGslamAGCondAllObjs 1 }

	gslamAGCondAllEntry OBJECT-TYPE
		SYNTAX  		GslamAGCondAllEntry
		MAX-ACCESS		not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG standing conditions table."
		INDEX { gslamAGCondAllCondType, gslamAGCondAllSeqNum }
	    ::= { gslamAGCondAllTable 1 }

	GslamAGCondAllEntry ::= SEQUENCE {
		gslamAGCondAllCondType	ConditionType,
		gslamAGCondAllSeqNum	Unsigned32,
		gslamAGCondAllEqptAddr	OBJECT IDENTIFIER,
		gslamAGCondAllEqptType	EquipmentTypeValue,
		gslamAGCondAllCondCode	INTEGER,
		gslamAGCondAllSrvEff	INTEGER,
		gslamAGCondAllTimeStamp	DateAndTime,
		gslamAGCondAllDirn		INTEGER,
		gslamAGCondAllCondDescr	OCTET STRING
	}

	gslamAGCondAllCondType	OBJECT-TYPE
		SYNTAX		ConditionType
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate the condition type of the event."
	    ::= { gslamAGCondAllEntry 1 }

	gslamAGCondAllSeqNum		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The sequence number of the entry in this condition table."
	    ::= { gslamAGCondAllEntry 2 }

	gslamAGCondAllEqptAddr	OBJECT-TYPE
		SYNTAX		OBJECT IDENTIFIER
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate the OID of the physical entity involved in this event."
	    ::= { gslamAGCondAllEntry 3 }

	gslamAGCondAllEqptType	OBJECT-TYPE
		SYNTAX		EquipmentTypeValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate the type of the physical entity involved in this event."
	    ::= { gslamAGCondAllEntry 4 }

	gslamAGCondAllCondCode	OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			cleared(2),
			critical(3),
			major(4),
			minor(5),
			not-alarmed(6),
			not-reported(7)
		}
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate the severity of the alarm (if applicable) for the event."
	    ::= { gslamAGCondAllEntry 5 }

	gslamAGCondAllSrvEff	OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			service-affected(2),
			not-service-affected(3),
			not-applicable(4)
		}
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate the service effect of this event."
	    ::= { gslamAGCondAllEntry 6 }

	gslamAGCondAllTimeStamp	OBJECT-TYPE
		SYNTAX		DateAndTime
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the timestamp when this event occured."
	    ::= { gslamAGCondAllEntry 7 }

	gslamAGCondAllDirn	OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			rcv(2),
			trmt(3),
			rcv-trmt(4),
			not-present(5)
		}
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate the direction of the event."
	    ::= { gslamAGCondAllEntry 8 }

	gslamAGCondAllCondDescr	OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(1..64))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Condition description of the event."
	    ::= { gslamAGCondAllEntry 9 }

