	    ::= { gslamAGAlarmModuleEntry 11 }

	--UV 5.0 START

	-- alarm module objects
	gslamAGREL50AlarmModuleTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGREL50AlarmModuleEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the alarm modules of GSLAM-AG."
	    ::= { ipiGslamAGAlarmModuleObjs 2 }

