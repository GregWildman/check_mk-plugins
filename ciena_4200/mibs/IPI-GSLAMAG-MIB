	gslamAGShelfEntry OBJECT-TYPE
		SYNTAX  		GslamAGShelfEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in shelf table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGShelfIndex }
	    ::= { gslamAGShelfTable 1 }

	GslamAGShelfEntry ::= SEQUENCE {
		gslamAGShelfIndex	Unsigned32,
		gslamAGShelfType		EquipmentTypeValue,
		gslamAGShelfLocation	LocationValue,
		gslamAGShelfCLEI			CLEIValue,
		gslamAGShelfPrimaryState	PrimaryStateValue,
		gslamAGShelfCtrlModuleCount	Unsigned32,
		gslamAGShelfIOModuleCount		Unsigned32,
		gslamAGShelfPwrModuleCount	Unsigned32,
		gslamAGShelfFanModuleCount		Unsigned32,
		gslamAGShelfPrimarySyncModIndex		Unsigned32,
		gslamAGShelfSecondarySyncModIndex	Unsigned32,
		gslamAGShelfActiveSyncModIndex		Unsigned32,
		gslamAGShelfSyncSwitchCmd		SyncSwitchCmdValue,
		gslamAGShelfSyncState			TimeSyncStateValue,
		gslamAGShelfInterModuleTimeSlotAvail	OCTET STRING,
		gslamAGShelfFeatureCode			OCTET STRING,
		gslamAGShelfFeatureKey			OCTET STRING,
		gslamAGShelfSerialNumber		OCTET STRING,
                gslamAGShelfConsoleIF			ConsoleIFValue,
                gslamAGShelfConsolePortState		PortStateValue
	}

