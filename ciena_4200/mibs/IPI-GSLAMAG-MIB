	-- ESOM SUPPORT ENDS

   -- GSLAM-AG fabric connection objects.
	gslamAGFabricConnectionTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGFabricConnectionEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the all the fabric connection of GSLAM-AG."
	    ::= { ipiGslamAGFabricConnectionObjs 1 }

	gslamAGFabricConnectionEntry OBJECT-TYPE
		SYNTAX  		GslamAGFabricConnectionEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG fabric connection table."
		INDEX   { gslamAGFabricConnectionFromIfIndex, gslamAGFabricConnectionToIfIndex }
	    ::= { gslamAGFabricConnectionTable 1 }

	GslamAGFabricConnectionEntry ::= SEQUENCE {
		gslamAGFabricConnectionFromIfIndex	Unsigned32,
		gslamAGFabricConnectionToIfIndex	Unsigned32,
		gslamAGFabricConnectionRowStatus	RowStatus,
		gslamAGFabricConnectionDirection	ConnModeValue,
		gslamAGFabricConnectionCktID		OCTET STRING,
		gslamAGFabricConnectionLPID			OCTET STRING
	}

	gslamAGFabricConnectionFromIfIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The first index of GSLAM-AG fabric connection table. It indicates
			the 'from' part of ifIndex in the fabric connection."
	    ::= { gslamAGFabricConnectionEntry 1 }

	gslamAGFabricConnectionToIfIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The second index of GSLAM-AG fabric connection table. It indicates
			the 'to' part of ifIndex in the fabric connection. Each ifIndex is a 32-bit
			unsigned integer. From left (most significant) to right, we allocate 4
			bits for unit(shelf) id, 4 bits for IO module id, 4 bits for interface(port)
			id, 4 bits for channel id, and the rest (rightmost) 16 bits for OPVC group."
	    ::= { gslamAGFabricConnectionEntry 2 }

	gslamAGFabricConnectionRowStatus		OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"Current status of this GSLAM-AG fabric connection entry."
	    ::= { gslamAGFabricConnectionEntry 3 }

	gslamAGFabricConnectionDirection	OBJECT-TYPE
		SYNTAX	ConnModeValue
 	 	MAX-ACCESS  read-write
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the direction of this fabric connection. For unidirection, traffic goes
				from 'from' to 'to'. Default value is bidirection"
 	 	::= { gslamAGFabricConnectionEntry 4 }

	gslamAGFabricConnectionCktID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The connection ID management application using for creating this connection, Return empty string if not available."
	    ::= { gslamAGFabricConnectionEntry 5 }

	gslamAGFabricConnectionLPID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The light path ID management application enters when creating this connection, Return empty string if not available."
	    ::= { gslamAGFabricConnectionEntry 6 }

	-- UV 5.0 CHANGE START 
	-- Fabric Cross Connect

	gslamAGREL50FabricCXTable		OBJECT-TYPE
