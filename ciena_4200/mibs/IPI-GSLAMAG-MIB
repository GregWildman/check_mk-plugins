	gslamAGOpticalDegreeTable	OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGOpticalDegreeEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the current status of
			 all optical degrees in the GSLAM-AG."
	    ::= { ipiGslamAGOpticalDegreeMgmtObjs 1 }

	gslamAGOpticalDegreeEntry OBJECT-TYPE
		SYNTAX  		GslamAGOpticalDegreeEntry
		MAX-ACCESS		not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG Optical Degree table."
		INDEX { gslamAGOpticalDegreeId }
	    ::= { gslamAGOpticalDegreeTable 1 }

	GslamAGOpticalDegreeEntry ::= SEQUENCE {
		gslamAGOpticalDegreeId			Unsigned32,
		gslamAGOpticalDegreeRowStatus		RowStatus,
		gslamAGOpticalDegreeName		OCTET STRING,
		gslamAGOpticalDegreeOSC			FlagValue,
		gslamAGOpticalDegreeAmplificationConfig		AmplificationConfigModeValue,
        gslamAGOpticalDegreeNominalPerChnlOPPwr     OCTET STRING,
        gslamAGOpticalDegreeNtwkFibType2            INTEGER,
        gslamAGOpticalDegreeTotalDispersion         INTEGER,
        gslamAGOpticalDegreeNtwkFibLen              INTEGER,
        gslamAGOpticalDegreeAPSDControl             INTEGER,
        gslamAGOpticalDegreeAPSDStatus              INTEGER 
	}

	gslamAGOpticalDegreeId	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Degree interface identifier for an optical interface."
	    ::= { gslamAGOpticalDegreeEntry 1 }

	gslamAGOpticalDegreeRowStatus		OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"Current status of this Optical Degree entry."
    	::= { gslamAGOpticalDegreeEntry 2 }

	gslamAGOpticalDegreeName	OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(1..63))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Degree interface name for an optical interface."
	    ::= { gslamAGOpticalDegreeEntry 3 }

	gslamAGOpticalDegreeOSC	OBJECT-TYPE
		SYNTAX		FlagValue
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Indicate if optical interface provides the
			 Optical Supervisory Channel "
	    ::= { gslamAGOpticalDegreeEntry 4 }

	gslamAGOpticalDegreeAmplificationConfig	OBJECT-TYPE
		SYNTAX		AmplificationConfigModeValue
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Defines the optical amplifier configuration for the interface "
	    ::= { gslamAGOpticalDegreeEntry 5 }

    gslamAGOpticalDegreeNominalPerChnlOPPwr  OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(1..63))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            " The nominal per channel output power for the degree.
                 -17.0 to +13.0 (dBm). Default Value = 1"
        ::= { gslamAGOpticalDegreeEntry 6 }

    gslamAGOpticalDegreeNtwkFibType2 OBJECT-TYPE
        SYNTAX      INTEGER {
            unknown(1),
            unprovisioned(2),
            g652(3),
            g653(4),
            g655EL(5),
            g655LS(6),
            g655TW(7)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This is the fiber type. Default is unprovisioned"
        ::= { gslamAGOpticalDegreeEntry 7 }

