        SYNTAX      INTEGER (-1500..3000)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            " The optical total dispersion.
             -1500 to 3000 (ps/nm). Default Value = 1. No Default"
        ::= { gslamAGOpticalDegreeEntry 8 }

    gslamAGOpticalDegreeNtwkFibLen OBJECT-TYPE
        SYNTAX      INTEGER (0..200)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This is the fiber length. Default is 0"
        ::= { gslamAGOpticalDegreeEntry 9 }

     gslamAGOpticalDegreeAPSDControl OBJECT-TYPE
        SYNTAX      INTEGER {
            unknown (1),
            enabled (2),
            disabled (3),
            manrestart(4)
        }
        MAX-ACCESS  read-write
        STATUS      current
