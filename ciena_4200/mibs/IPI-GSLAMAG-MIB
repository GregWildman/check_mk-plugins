		gslamAGREL50ROADMOpticalCXSrcSlot	Unsigned32,
		gslamAGREL50ROADMOpticalCXSrcPort	Unsigned32, 
		gslamAGREL50ROADMOpticalCXSrcChnl	Unsigned32,
		gslamAGREL50ROADMOpticalCXDstShelf	Unsigned32, 
		gslamAGREL50ROADMOpticalCXDstSlot	Unsigned32, 
		gslamAGREL50ROADMOpticalCXDstPort	Unsigned32,
		gslamAGREL50ROADMOpticalCXDstChnl	Unsigned32, 
		gslamAGREL50ROADMOpticalCXRowStatus  RowStatus,
		gslamAGREL50ROADMOpticalCXDirection	ConnModeValue,
		gslamAGREL50ROADMOpticalCXCktID		OCTET STRING,
		gslamAGREL50ROADMOpticalCXNature    INTEGER,
		gslamAGREL50ROADMOpticalCXItuNumber  Unsigned32,
		gslamAGREL50ROADMOpticalCXsrcDegree  INTEGER,
		gslamAGREL50ROADMOpticalCXdstDegree  INTEGER

	}

	gslamAGREL50ROADMOpticalCXSrcShelf  OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
