				rls-lpbk-term(13),
				rls-lpbk-crs(14),
				remove(15),
				restore(16),
				init-level7(17),
				init-level8(18)
			}

        ProtectionSwitchCmdValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Protection Switch command issued against a entity. i.e port/channel/opvcx."
		SYNTAX
			INTEGER {
				release-protected(1),
				release-protecting(2),
				lockout-of-protection(3),
				forced-switch-from-protected(4),
				forced-switch-from-protecting(5),
				manual-switch-from-protected(6),
				manual-switch-from-protecting(7)
			}

	FlagValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the unknown/off/on state of a particular flag."
		SYNTAX
			INTEGER {
				unknown(1),
				off(2),
				on(3),
				not-applicable(4)
			}

	DispValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the dispersion value of a Port association."
		SYNTAX
			INTEGER {
				zero(0),
            	ten(10),
            	twenty(20),
            	thirty(30),
            	fourty(40),
            	fifty(50),
            	sixty(60),
            	seventy(70),
            	eighty(80),
            	ninety(90),
            	hundred(100),
            	hundred-ten(110),
            	hundred-twenty(120),
            	hundred-fourty(140),
            	hundred-fifty(150),
            	three-hundred(300)
			}

	IfDirValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the interface direction: customer facing or network facing."
		SYNTAX
			INTEGER {
				unknown(1),
				drop(2), -- customer facing
				line(3),  -- networking facing
				not-applicable(4)
			}

	TcvrReachValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the range of transciever in the port."
		SYNTAX
			INTEGER {
				unknown(1),
				short-reach(2),
				intermediate-reach(3),
				long-reach(4),
				ultra-long-reach(5),
				not-applicable(6)
			}

	ShelfTypeValue ::=	TEXTUAL-CONVENTION
