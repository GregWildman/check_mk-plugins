     -- GSLAM-AG facility/path protection group objects.
	gslamAGProtectionGroupTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGProtectionGroupEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the facility/path protection group of GSLAM-AG."
	    ::= { ipiGslamAGProtectionGroupObjs 1 }

	gslamAGProtectionGroupEntry OBJECT-TYPE
		SYNTAX  		GslamAGProtectionGroupEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG protection group table."
		INDEX   { gslamAGProtectionGroupProtectedIndex, gslamAGProtectionGroupProtectingIndex }
	    ::= { gslamAGProtectionGroupTable 1 }

	GslamAGProtectionGroupEntry ::= SEQUENCE {
		gslamAGProtectionGroupProtectedIndex	Unsigned32,
		gslamAGProtectionGroupProtectingIndex	Unsigned32,
		gslamAGProtectionGroupRowStatus		RowStatus,
		gslamAGProtectionGroupSwitchType	ProtectionSwitchTypeValue,
		gslamAGProtectionGroupSwitchDir		ProtectionSwitchDirValue,
		gslamAGProtectionGroupRvrt		FlagValue,
		gslamAGProtectionGroupLPID		OCTET STRING,
		gslamAGProtectionGroupSwitchCmd		ProtectionSwitchCmdValue,
		gslamAGProtectionGroupActiveIndex	Unsigned32
	}

	gslamAGProtectionGroupProtectedIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The index value of the first entity (port, channel or opvcx) in the protection group."
	    ::= { gslamAGProtectionGroupEntry 1 }

	gslamAGProtectionGroupProtectingIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The index value of the second entity (port, channel or opvcx) in the protection group."
	    ::= { gslamAGProtectionGroupEntry 2 }


	gslamAGProtectionGroupRowStatus		OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"Current status of this protection group entry."
	    ::= { gslamAGProtectionGroupEntry 3 }

	gslamAGProtectionGroupSwitchType		OBJECT-TYPE
		SYNTAX		ProtectionSwitchTypeValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicate the protection switch type of this protection entry."
	    ::= { gslamAGProtectionGroupEntry 4 }

	gslamAGProtectionGroupSwitchDir		OBJECT-TYPE
		SYNTAX		ProtectionSwitchDirValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicate the protection switch direction of this protection entry."
	    ::= { gslamAGProtectionGroupEntry 5 }

	gslamAGProtectionGroupRvrt		OBJECT-TYPE
		SYNTAX		FlagValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicate if it is protection revertable of this protection entry."
	    ::= { gslamAGProtectionGroupEntry 6 }

	gslamAGProtectionGroupLPID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The light path ID management application enters when creating 
				this protection group, return empty string if not available."
	    ::= { gslamAGProtectionGroupEntry 7 }

	gslamAGProtectionGroupSwitchCmd     OBJECT-TYPE
		SYNTAX      ProtectionSwitchCmdValue
		MAX-ACCESS  read-write
		STATUS      current
		DESCRIPTION
	    		"Indicate command issued to this GSLAM-AG protection group."
	    ::= { gslamAGProtectionGroupEntry 8 }

	gslamAGProtectionGroupActiveIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"The ifIndex value of the working entity (port, channel or opvcx) in the protection group."
	    ::= { gslamAGProtectionGroupEntry 9 }

     -- UV 5.0 CHANGE START 
     -- GSLAM-AG facility/path protection group objects.
	gslamAGREL50PrGrTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGREL50PrGrEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the facility/path protection group of GSLAM-AG."
	    ::= { ipiGslamAGProtectionGroupObjs 2 }

	gslamAGREL50PrGrEntry OBJECT-TYPE
		SYNTAX  		GslamAGREL50PrGrEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG protection group table."
		INDEX   { 	gslamAGREL50PrGrProtectedSIndex, 
			  		gslamAGREL50PrGrProtectedMIndex,
			  		gslamAGREL50PrGrProtectedPIndex,
                    gslamAGREL50PrGrProtectedCIndex,
			  		gslamAGREL50PrGrProtectedTIndex,
                    gslamAGREL50PrGrProtectingSIndex,
			  		gslamAGREL50PrGrProtectingMIndex,
                    gslamAGREL50PrGrProtectingPIndex,
			  		gslamAGREL50PrGrProtectingCIndex,
                    gslamAGREL50PrGrProtectingTIndex}
	    ::= { gslamAGREL50PrGrTable 1 }

	GslamAGREL50PrGrEntry ::= SEQUENCE {
		gslamAGREL50PrGrProtectedSIndex	     		Unsigned32,
		gslamAGREL50PrGrProtectedMIndex				Unsigned32,
		gslamAGREL50PrGrProtectedPIndex	        	Unsigned32,
		gslamAGREL50PrGrProtectedCIndex	        	Unsigned32,
		gslamAGREL50PrGrProtectedTIndex				Unsigned32,
		gslamAGREL50PrGrProtectingSIndex	       	Unsigned32,
		gslamAGREL50PrGrProtectingMIndex			Unsigned32,
		gslamAGREL50PrGrProtectingPIndex	       	Unsigned32,
		gslamAGREL50PrGrProtectingCIndex	       	Unsigned32,
		gslamAGREL50PrGrProtectingTIndex	       	Unsigned32,
		gslamAGREL50PrGrRowStatus		        	RowStatus,
		gslamAGREL50PrGrSwitchType	               	ProtectionSwitchTypeValue,
		gslamAGREL50PrGrSwitchDir		        	ProtectionSwitchDirValue,
		gslamAGREL50PrGrRvrt		               	FlagValue,
		gslamAGREL50PrGrLPID		               	OCTET STRING,
		gslamAGREL50PrGrSwitchCmd		        	ProtectionSwitchCmdValue,
		gslamAGREL50PrGrActiveSIndex	           	Unsigned32,
		gslamAGREL50PrGrActiveMIndex	           	Unsigned32,
		gslamAGREL50PrGrActivePIndex	           	Unsigned32,
		gslamAGREL50PrGrActiveCIndex	           	Unsigned32,
		gslamAGREL50PrGrActiveTIndex	           	Unsigned32
	}

	gslamAGREL50PrGrProtectedSIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The index1 value of the shelf in the protected group."
	    ::= { gslamAGREL50PrGrEntry 1 }

	gslamAGREL50PrGrProtectedMIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The index2 value of the module in the protected group."
	    ::= { gslamAGREL50PrGrEntry 2 }

	gslamAGREL50PrGrProtectedPIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The index3 value of the port in the protected group."
	    ::= { gslamAGREL50PrGrEntry 3 }

	gslamAGREL50PrGrProtectedCIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The index4 value of the chnl in the protected group."
	    ::= { gslamAGREL50PrGrEntry 4 }

	gslamAGREL50PrGrProtectedTIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The index5 value of the ts in the protected group."
	    ::= { gslamAGREL50PrGrEntry 5 }

