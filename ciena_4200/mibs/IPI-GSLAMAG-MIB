	GslamAGREL50ROADMPortAssocEntry ::= SEQUENCE {
		gslamAGREL50ROADMPortAssocSrcShelf  		Unsigned32,
		gslamAGREL50ROADMPortAssocSrcSlot			Unsigned32,
		gslamAGREL50ROADMPortAssocSrcPort			Unsigned32,
		gslamAGREL50ROADMPortAssocDstShelf			Unsigned32,
		gslamAGREL50ROADMPortAssocDstSlot			Unsigned32,
		gslamAGREL50ROADMPortAssocDstPort			Unsigned32,
		gslamAGREL50ROADMPortAssocRowStatus    	RowStatus,
		gslamAGREL50ROADMPortAssocType				INTEGER,
		gslamAGREL50ROADMPortAssocDCMDispersion		DispValue,
		gslamAGREL50ROADMPortAssocDCMType			INTEGER,
		gslamAGREL50ROADMPortAssocPadAttenuation	INTEGER
	}
	
	gslamAGREL50ROADMPortAssocSrcShelf  OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The src shelf of the port association."
        ::= { gslamAGREL50ROADMPortAssocEntry  1 }
	
