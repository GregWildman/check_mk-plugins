            gslamAGREL50ConcOPVCxReceivedTrc     			OCTET STRING,    -- INCTRC
            gslamAGREL50ConcOPVCxCRSLpbkType                INTEGER,
            gslamAGREL50ConcOPVCxCRSLpbkDuration            Unsigned32
            -- gslamAGREL50ConcOPVCxRowStatus    			RowStatusValue
