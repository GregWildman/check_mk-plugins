		gslamAGREL50AlarmModuleSecondaryState			SecondaryStateValue,
		gslamAGREL50AlarmModuleHWManufactureName		HWManufactureNameValue,
		gslamAGREL50AlarmModuleHWManufactureSerial	HWManufactureSerialValue,
		gslamAGREL50AlarmModuleHWRevision				HWRevisionValue,
		gslamAGREL50AlarmModuleHWManufacturePartName  HWPartNumberValue,
		gslamAGREL50AlarmModuleHWManufacturePartNumber	HWPartNumberValue
	}

	gslamAGREL50AlarmModuleIndex	OBJECT-TYPE
