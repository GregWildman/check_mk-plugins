IPI-GSLAMAG-MIB DEFINITIONS ::= BEGIN

IMPORTS
	MODULE-IDENTITY, OBJECT-TYPE
		FROM SNMPv2-SMI
	TEXTUAL-CONVENTION, RowStatus, DateAndTime
		FROM SNMPv2-TC
	ipiProductsModules
		FROM IPI-GLOBAL-REG
	ipiGslamAG
		FROM IPI-PRODUCTS-MIB
	snmpTrapServerIP, snmpTrapServerVersion, snmpTrapServerCommunity,
	snmpTrapServerPort, snmpServerCommunityHost, snmpServerCommunityValue,
	snmpServerCommunityAccessType, nameServerIP, domainServerName,
	radiusServerIP, radiusServerSecret, radiusServerTimeout, ntpServerIP,
	ntpServerKey, ntpServerVersion, ntpAuthKeyId,
	ntpAuthKeyValue, ntpTrustedKeyId, snmpServiceMode,cnOSPFAreaSizeAreaId
		FROM IPI-SERVICES-MIB
  	ospfRouterId, ospfIfIpAddress, ospfAddressLessIf, ospfIfState,
    	ospfVirtIfAreaId, ospfVirtIfNeighbor, ospfVirtIfState,
    	ospfNbrIpAddr, ospfNbrAddressLessIndex, ospfNbrRtrId,
    	ospfNbrState, ospfVirtNbrArea, ospfVirtNbrRtrId, ospfVirtNbrState,
    	ospfLsdbType, ospfLsdbLsid, ospfLsdbRouterId, ospfLsdbAreaId,
    	ospfExtLsdbLimit, ospf,
	ospfPacketSrc, ospfConfigErrorType, ospfPacketType
        	FROM OSPF-MIB
    	oampDevAuthStatus, oampDevAuthDeviceId
        	FROM IPI-OAMP-MIB;


cn4200MibModule 	MODULE-IDENTITY
	LAST-UPDATED 	"0405051400Z"
	ORGANIZATION	"CIENA Corporation"
	CONTACT-INFO
		"CIENA Support Services Contact Information

	Domestic Hotline Number - 1-800-CIENA24 (243-6224)
	International Hotline Number - Europe: 44 207 012 5500
				- Asia: 81 3 3248 4680

	Email		   - CIENA24@ciena.com
	Web		     - www.ciena.com/support/

	RMA Shipping Address:   CIENA Corporation
					900 International Drive
					Linthicum, MD  USA 21090
		 "
	DESCRIPTION
		"Copyright(C) Ciena Corporation, Inc. (2004-2006).
		This version of this MIB module is part of the Ciena enterprise
		MIBs for the CN4200 product."
	::= { ipiProductsModules 9 }

	PrimaryStateValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the primary state and its qualifier for entity."
		SYNTAX
			INTEGER {
				unknown(1),
				is-nr(2),
				oos-au(3),
				oos-ma(4),
				oos-auma(5)
			}


	SecondaryStateValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"Indicate the primary state and its qualifier for entity."
		SYNTAX
			INTEGER {
				unknown(1),
				none(2),		-- not applicable
				ains (3),	-- auto in service
				ains-faf (4),	-- auto in service, facility failure
				ains-flt (5),	-- unassigned, fault
				ains-mea (6),	-- auto in service, mismatch of eqpt and attr
				ains-ueq (7),  -- unassigned, unequipped
				busy (8),		-- busy
				flt (9),	-- fault
				flt-busy (10),	-- fault, busy
				flt-idle (11),	-- fault, idle
				flt-mt (12),	-- fault, maintenance
				frcd (13),		-- forced back into service
				idle (14),		-- idle
				idle-busy (15),	-- idle, busy
				mea (16),	-- mismatch of eqpt and attr
				mt (17),	-- maintenance
				mt-busy (18),	-- maintenance, busy
				mt-busy-lpbk (19),	-- maintenance, busy, loopback
				mt-idle (20),	-- maintenance, idle
				mt-idle-lpbk (21),	-- maintenance, idle, loopback
				mt-ueq (22),	-- maintenance, unequipped
				psi (23),		-- protection switch inhibited. Also called 'lkdo'
				stbyc (24),	-- standby cold
				stbyh (25),	-- standby hot
				uas-eq (26),	-- unassigned, equipped
				uas-flt (27),		-- unassigned, fault
				uas-sgeo (28),	-- unassigned, supporting entity outage
				uas-ueq (29),	-- unassigned, unequipped
				ueq (30),	-- unequipped
				wrk (31),	-- working
				flt-mt-lpbk (32),	-- fault, maintenance, loopback
				mt-lpbk (33),	-- maintenance, loopback
				mt-ueq-lpbk (34),	-- maintenance, unequipped, loopback
				lpbk (35),       -- loopback
				busy-stbyc(36),	-- busy, standby cold
				busy-stbyh(37),	-- busy, standby hot
				busy-wrk(38),	-- busy, working
				idle-stbyc(39),	-- idle, standby cold
				idle-stbyh(40),	-- idle, standby hot
				idle-wrk(41),	-- idle, working
				uas(42),	-- unassigned
				ains-busy(43),	-- auto-in-service,busy
				busy-mt-flt(44),	-- busy, maintenance, fault
				degraded-stbyc(45),	-- degraded, standby cold
				degraded-stbyh(46),	-- degraded, standby hot
				degraded-wrk(47),	-- degraded, working
				busy-man(48),		-- busy, manual
				busy-degraded-wrk(49),	-- busy, degraded, working
				busy-lkdo(50),		-- busy, lockedout of protection
				busy-frcd(51),		-- busy, forced back to service
				busy-degraded-stbyh(52), -- busy, degraded, standby hot
				busy-flt-lkdo(53),	 -- busy, fault, lockedout of protection
				busy-flt-frcd(54),	 -- busy, fault, forced back to service
				ains-busy-lkdo(55),	 -- auto in service, busy, lockedout of protection
				ains-busy-frcd(56),	 -- auto in service, busy, forced back to service
				ains-lkdo(57),		 -- auto in service, lockedout of protection
				faf-lkdo(58),		 --  facility failure, locked out of protection
				mea-frcd(59),		 -- mismatch of equipment, forced back to service
				mea-lkdo(60),		 --  mismatch of equipment, locked out of protection
				idle-uas(61),		-- idle, unassigned
				man(62),		-- manual
				flt-frcd(63),		-- fault, forced back into service
				flt-psi(64),		-- fault, protection switch inhibited
                ains-busy-lpbk (65)  -- ains, busy, loopback
			}


	ActionValue ::=	TEXTUAL-CONVENTION
		STATUS	current
		DESCRIPTION
			"A command issued against a physical entity."
		SYNTAX
			INTEGER {
				init-level3(1),
				init-level6(2),
				init-level9(3),
				alw-swdx(4),
				inh-swdx(5),
				swdx-normal(6),
				swdx-forced(7),
				init-reg(8),
				opr-lpbk-facility(9),
				opr-lpbk-term(10),
				opr-lpbk-crs(11),
