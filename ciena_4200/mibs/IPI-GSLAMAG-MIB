     ::= { gslamAGREL50ConcOPVCxTable 1 }

	 GslamAGREL50ConcOPVCxEntry ::= SEQUENCE {
		    gslamAGREL50ConcOPVCxSIndex          			Unsigned32,
            gslamAGREL50ConcOPVCxMIndex          			Unsigned32,
            gslamAGREL50ConcOPVCxPIndex          			Unsigned32,
            gslamAGREL50ConcOPVCxCIndex          			Unsigned32,
            gslamAGREL50ConcOPVCxTIndex          			Unsigned32,
            --gslamAGREL50ConcOPVCxIfIndex       			Unsigned32,
            gslamAGREL50ConcOPVCxType            			EquipmentTypeValue,
            gslamAGREL50ConcOPVCxAction          			ActionValue,
            gslamAGREL50ConcOPVCxPrimaryState    			PrimaryStateValue,
            gslamAGREL50ConcOPVCxSecondaryState  			SecondaryStateValue,
            gslamAGREL50ConcOPVCxProtFunction    			ProtectionEntityValue,
            gslamAGREL50ConcOPVCxSESThreshold    			Unsigned32,     -- SESTH
            gslamAGREL50ConcOPVCxCSESThreshold   			Unsigned32,       -- CSESTH
            gslamAGREL50ConcOPVCxDegMonLev       			Unsigned32,     -- DEGM
            gslamAGREL50ConcOPVCxDegThreshold    			Integer32,        -- DEGTHR
            gslamAGREL50ConcOPVCxOCIProtSwitch   			FlagValue,            -- OCIPS
            gslamAGREL50ConcOPVCxTIMDetectMode   			TIMModeValue,   -- TIMDETM
            gslamAGREL50ConcOPVCxTIMActDisable      		FlagValue,        -- TIMACTDIS
            gslamAGREL50ConcOPVCxExpSAPI            		OCTET STRING,   -- EXPSAPI
            gslamAGREL50ConcOPVCxExpDAPI            		OCTET STRING,   -- EXPDAPI
            gslamAGREL50ConcOPVCxExpOperSpecified   		OCTET STRING,   -- EXPOPR
            gslamAGREL50ConcOPVCxTrcSAPI         			OCTET STRING,   -- TRCSAPI
            gslamAGREL50ConcOPVCxTrcDAPI         			OCTET STRING,   -- TRCDAPI
            gslamAGREL50ConcOPVCxTrcOperSpecified   		OCTET STRING,   -- TRCOPR
            gslamAGREL50ConcOPVCxReceivedTrcSAPI    		OCTET STRING,
            gslamAGREL50ConcOPVCxReceivedTrcDAPI    		OCTET STRING,
