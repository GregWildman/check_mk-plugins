	-- *********** R5.0 RTRV-TOPOLOGY *******************
	gslamAGREL50TopologyTable	OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGREL50TopologyEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the current status of
			 all optical topologies in the GSLAM-AG."
	    ::= { ipiGslamAGREL50TopologyObjs 1 }

	gslamAGREL50TopologyEntry OBJECT-TYPE
		SYNTAX  		GslamAGREL50TopologyEntry
		MAX-ACCESS		not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG Optical Topology table."
		INDEX { gslamAGREL50TopologyShelf,gslamAGREL50TopologyModule,
				gslamAGREL50TopologyPort, gslamAGREL50TopologyChnl }
	    ::= { gslamAGREL50TopologyTable 1 }

	GslamAGREL50TopologyEntry ::= SEQUENCE {
		gslamAGREL50TopologyShelf		Unsigned32,
		gslamAGREL50TopologyModule		Unsigned32,
		gslamAGREL50TopologyPort		Unsigned32,
		gslamAGREL50TopologyChnl		Unsigned32,
		gslamAGREL50TopologyDir			INTEGER,
		gslamAGREL50TopologyRX        	OCTET STRING,
		gslamAGREL50TopologyTX        	OCTET STRING,
		gslamAGREL50TopologyProtectionInfo		INTEGER,
		gslamAGREL50TopologyRXProtection   	OCTET STRING,
		gslamAGREL50TopologyTXProtection   	OCTET STRING
	}

	gslamAGREL50TopologyShelf OBJECT-TYPE
       	SYNTAX      Unsigned32
       	MAX-ACCESS  not-accessible
       	STATUS      current
       	DESCRIPTION
			"The src shelf aid"
       	::= { gslamAGREL50TopologyEntry 1 }

	gslamAGREL50TopologyModule OBJECT-TYPE
       	SYNTAX      Unsigned32
       	MAX-ACCESS  not-accessible
       	STATUS      current
       	DESCRIPTION
			"The src module aid"
       	::= { gslamAGREL50TopologyEntry 2 }

	gslamAGREL50TopologyPort OBJECT-TYPE
       	SYNTAX      Unsigned32
       	MAX-ACCESS  not-accessible
       	STATUS      current
       	DESCRIPTION
			"The src port aid"
       	::= { gslamAGREL50TopologyEntry 3 }

	gslamAGREL50TopologyChnl OBJECT-TYPE
       	SYNTAX      Unsigned32
       	MAX-ACCESS  not-accessible
       	STATUS      current
       	DESCRIPTION
			"The src chnl aid"
       	::= { gslamAGREL50TopologyEntry 4 }

	gslamAGREL50TopologyDir OBJECT-TYPE
		SYNTAX  INTEGER {
            unknown(1),
            not-applicable(2),
            none(3),
            rx(4),
            tx(5),
            rx-tx(6)
        }
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The direction aid"
       	::= { gslamAGREL50TopologyEntry 5 }

	gslamAGREL50TopologyRX OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The RX side topology chain. It contains the following
				(inporttype-shelf-module-port,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-shelf-module-port)+|
				where portype is a numeric val,
					  shelf,module and port is numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a RX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-1-2-1|X-1-2-1,ModType,0,0,0,0,X-1-2-2|..."
       	::= { gslamAGREL50TopologyEntry 6 }

	gslamAGREL50TopologyTX OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The TX side topology chain. It contains the following
				(inporttype-shelf-module-port,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-shelf-module-port)+|
				where portype is a numeric val,
					  shelf,module and port is numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a TX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-1-2-1|X-1-2-1,Mod enum,0,0,0,0,0,X-1-2-2|..."
       	::= { gslamAGREL50TopologyEntry 7 }

	gslamAGREL50TopologyProtectionInfo OBJECT-TYPE
		SYNTAX  INTEGER {
			unknown(0),
            no(1),
            yes(2)
        }
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"This provides info to NE if protection data is to be 
			 provided (yes) or not. Working data is always provided. "
       	::= { gslamAGREL50TopologyEntry 8 }

	gslamAGREL50TopologyRXProtection OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The RX side protection topology chain. It contains the following
				(inporttype-shelf-module-port,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-shelf-module-port)+|
				where portype is a numeric val,
					  shelf,module and port is numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a RX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-1-2-1|X-1-2-1,ModType,0,0,0,0,X-1-2-2|..."
       	::= { gslamAGREL50TopologyEntry 9 }

	gslamAGREL50TopologyTXProtection OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..4096))
       	MAX-ACCESS  read-only
       	STATUS      current
       	DESCRIPTION
			"The TX side topology chain. It contains the following
				(inporttype-shelf-module-port,
					Device,
					Degree,
					Dcmtype,
					Dispersion,
					padval,
					out-porttype-shelf-module-port)+|
				where portype is a numeric val,
					  shelf,module and port is numeric val,
					  device is either F for Fiber or numeric val for a module
					  Degree is numeric
					  Dcmtype is numeric
					  Dispersion is numeric
					  padval is numeric
				For example for a TX chain like,
				N-1X2-3Y4-N
			 The value contained in this would be something like
				N,F,1,1,10,0,X-1-2-1|X-1-2-1,Mod enum,0,0,0,0,0,X-1-2-2|..."
       	::= { gslamAGREL50TopologyEntry 10}

	-- ROADM 5.0 START
