		DESCRIPTION
			"A table about the all the logical connection Numic of ESOM."
	    ::= { ipiGslamAGLogicalConnectionObjs 2 }

	gslamAGREL50LogicalCXEntry OBJECT-TYPE
		SYNTAX  		GslamAGREL50LogicalCXEntry
		MAX-ACCESS		not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the ESOM logicalConnection Numic table."
		INDEX   { 	gslamAGREL50LogicalCXFromSIndex,gslamAGREL50LogicalCXFromMIndex,
			  		gslamAGREL50LogicalCXFromPIndex,gslamAGREL50LogicalCXFromCIndex,
			  		gslamAGREL50LogicalCXFromTIndex,gslamAGREL50LogicalCXToSIndex,
			  		gslamAGREL50LogicalCXToMIndex,gslamAGREL50LogicalCXToPIndex,
			  		gslamAGREL50LogicalCXToCIndex,gslamAGREL50LogicalCXToTIndex}
	    ::= { gslamAGREL50LogicalCXTable 1 }

	GslamAGREL50LogicalCXEntry ::= SEQUENCE {
		gslamAGREL50LogicalCXFromSIndex      Unsigned32,
		gslamAGREL50LogicalCXFromMIndex      Unsigned32,
		gslamAGREL50LogicalCXFromPIndex      Unsigned32,
		gslamAGREL50LogicalCXFromCIndex      Unsigned32,
		gslamAGREL50LogicalCXFromTIndex      Unsigned32,
		gslamAGREL50LogicalCXToSIndex        Unsigned32,
	        gslamAGREL50LogicalCXToMIndex        Unsigned32,
	        gslamAGREL50LogicalCXToPIndex        Unsigned32,
	        gslamAGREL50LogicalCXToCIndex        Unsigned32,
	        gslamAGREL50LogicalCXToTIndex        Unsigned32,
		gslamAGREL50LogicalCXRowStatus       RowStatus,
		gslamAGREL50LogicalCXDirection	    ConnModeValue,
		gslamAGREL50LogicalCXCktID		    OCTET STRING
		}

	gslamAGREL50LogicalCXFromSIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index 1 of ESOM GslamAGREL50LogicalCXEntry table. 
			 It indicates shelf index of
			 the 'from' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 1 }

	gslamAGREL50LogicalCXFromMIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index2 of ESOM logical ConnectionNumic table. It indicates module index of
			the 'from' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 2 }

	gslamAGREL50LogicalCXFromPIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index3 of ESOM logical ConnectionNumic table. It indicates port index of
			the 'from' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 3 }

	gslamAGREL50LogicalCXFromCIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index4 of ESOM logical ConnectionNumic table. It indicates chnl index of
			the 'from' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 4 }

	gslamAGREL50LogicalCXFromTIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index5 of ESOM logical ConnectionNumic table. 
			It indicates ts index of
			the 'from' part of ifIndex in the logical ConnectionNumic.
			It is a bit-encoded entity and '1' represents that TS is present
			and '0' represents not present."
	    ::= { gslamAGREL50LogicalCXEntry 5 }

	gslamAGREL50LogicalCXToSIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index1 of ESOM logical ConnectionNumic table. It indicates shelf index of
			the 'to' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 6 }

	gslamAGREL50LogicalCXToMIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index2 of ESOM logical ConnectionNumic table. It indicates Module index of
			the 'to' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 7 }

	gslamAGREL50LogicalCXToPIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index3 of ESOM logicalConnectionNumic table. It indicates port index of
			the 'to' part of ifIndex in the fabric ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 8 }

	gslamAGREL50LogicalCXToCIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index4 of ESOM logical ConnectionNumic table. It indicates chnl index of
			the 'to' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 9 }

	gslamAGREL50LogicalCXToTIndex			OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index5 of ESOM logical ConnectionNumic table. 
			It indicates ts index of
			the 'to' part of ifIndex in the logical ConnectionNumic."
	    ::= { gslamAGREL50LogicalCXEntry 10 }

	gslamAGREL50LogicalCXRowStatus			OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"Current status of this ESOM logical ConnectionNumic entry."
	    ::= { gslamAGREL50LogicalCXEntry 11 }

	gslamAGREL50LogicalCXDirection			OBJECT-TYPE
		SYNTAX	ConnModeValue
 	 	MAX-ACCESS  read-write
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the direction of this logical ConnectionNumic. For unidirection, traffic goes
				from 'from' to 'to'. Default value is bidirection"
 	 	::= { gslamAGREL50LogicalCXEntry 12 }

	gslamAGREL50LogicalCXCktID				OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The ConnectionNumic ID management application using for creating this ConnectionNumic, 
			 Return empty string if not available."
	    ::= { gslamAGREL50LogicalCXEntry 13 }

     -- UV 5.0 CHANGE	END
     
   -- GSLAM-AG neighbor connection objects.
	gslamAGExternalConnectionTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGExternalConnectionEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the all the neighbor connection info of GSLAM-AG."
	    ::= { ipiGslamAGExternalConnectionObjs 1}

	gslamAGExternalConnectionEntry OBJECT-TYPE
		SYNTAX  		GslamAGExternalConnectionEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG neighbor connection table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGShelfIndex, gslamAGIOModuleIndex, gslamAGPortIndex }
	    ::= { gslamAGExternalConnectionTable 1}

	GslamAGExternalConnectionEntry ::= SEQUENCE {
		gslamAGExternalConnectionLocalIP	IpAddress,
		gslamAGExternalConnectionLocalUMIC	UMICValue,
		gslamAGExternalConnectionRemoteNodeIP	IpAddress,
		gslamAGExternalConnectionRemoteIP	IpAddress,
		gslamAGExternalConnectionRemoteUMIC	UMICValue,
		gslamAGExternalConnectionRemoteNodeType	RemoteNodeTypeValue,
		gslamAGExternalConnectionLocalNodeTID	OCTET STRING,
		gslamAGExternalConnectionRemoteNodeTID	OCTET STRING,
		gslamAGExternalConnectionStatus		INTEGER,
		gslamAGExternalConnectionType		INTEGER,
		gslamAGExternalConnectionRemotePortAID	OCTET STRING
	}


	gslamAGExternalConnectionLocalIP		OBJECT-TYPE
		SYNTAX		IpAddress
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the IP address of the local end of this external connection."
	    ::= { gslamAGExternalConnectionEntry 1 }

	gslamAGExternalConnectionLocalUMIC		OBJECT-TYPE
		SYNTAX		UMICValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the UMIC value of the local end of this external connection."
	    ::= { gslamAGExternalConnectionEntry 2 }

	gslamAGExternalConnectionRemoteNodeIP		OBJECT-TYPE
		SYNTAX		IpAddress
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the IP address of the remote node of this external connection."
	    ::= { gslamAGExternalConnectionEntry 3 }

	gslamAGExternalConnectionRemoteIP	OBJECT-TYPE
		SYNTAX		IpAddress
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the IP address of the remote end of this external connection."
	    ::= { gslamAGExternalConnectionEntry 4 }

	gslamAGExternalConnectionRemoteUMIC	OBJECT-TYPE
		SYNTAX		UMICValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the UMIC value of the remote end of this external connection."
	    ::= { gslamAGExternalConnectionEntry 5 }

	gslamAGExternalConnectionRemoteNodeType		OBJECT-TYPE
		SYNTAX		RemoteNodeTypeValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the equiptment type of the remote end of this external connection."
	    ::= { gslamAGExternalConnectionEntry 6 }

	gslamAGExternalConnectionLocalNodeTID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(1..20))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the TID of the local end of this external connection."
	    ::= { gslamAGExternalConnectionEntry 7 }

	gslamAGExternalConnectionRemoteNodeTID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(1..20))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the TID of the remote end of this external connection."
	    ::= { gslamAGExternalConnectionEntry 8 }

	gslamAGExternalConnectionStatus	OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			ndp-enabled(2),
			admin-enabled(3)
		}
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate if the connection is ndp or admin enabled."
	    ::= { gslamAGExternalConnectionEntry 9 }

	gslamAGExternalConnectionType	OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			not-applicable(2),
			external(3),
			optical(4)
		}
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Used to indicate the connection type, if its optical connection ot not."
	    ::= { gslamAGExternalConnectionEntry 10 }

	gslamAGExternalConnectionRemotePortAID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(1..20))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicates the AID of the remote end port of this external connection."
	    ::= { gslamAGExternalConnectionEntry 11 }

	-- UV 5.0 start

