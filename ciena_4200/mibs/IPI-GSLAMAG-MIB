	-- ESOM Logical connection objects.
	gslamAGLogicalConnectionTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGLogicalConnectionEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the all the logical connections of ESOM."
	    ::= { ipiGslamAGLogicalConnectionObjs 1 }

	gslamAGLogicalConnectionEntry OBJECT-TYPE
		SYNTAX  		GslamAGLogicalConnectionEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in the GSLAM-AG fabric connection table."
		INDEX   { gslamAGLogicalConnectionFromIfIndex, gslamAGLogicalConnectionToIfIndex }
	    ::= { gslamAGLogicalConnectionTable 1 }

	GslamAGLogicalConnectionEntry ::= SEQUENCE {
		gslamAGLogicalConnectionFromIfIndex	Unsigned32,
		gslamAGLogicalConnectionToIfIndex	Unsigned32,
		gslamAGLogicalConnectionRowStatus	RowStatus,
		gslamAGLogicalConnectionDirection	ConnModeValue,
		gslamAGLogicalConnectionCktID		OCTET STRING
	}

	gslamAGLogicalConnectionFromIfIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The first index of ESOM logical connection table. It indicates
			the 'from' part of ifIndex in the fabric connection."
	    ::= { gslamAGLogicalConnectionEntry 1 }

	gslamAGLogicalConnectionToIfIndex		OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"The second index of ESOM logical connection table. It indicates
			the 'to' part of ifIndex in the logical connection. Each ifIndex is a 32-bit
			unsigned integer. From left (most significant) to right, we allocate 4
			bits for unit(shelf) id, 4 bits for IO module id, 8 bits for interface(port)
			id, 8 bits for channel id, and the rest (rightmost) 8 bits for subchannels."
	    ::= { gslamAGLogicalConnectionEntry 2 }

	gslamAGLogicalConnectionRowStatus		OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"Current status of this ESOM logical connection entry."
	    ::= { gslamAGLogicalConnectionEntry 3 }

	gslamAGLogicalConnectionDirection	OBJECT-TYPE
		SYNTAX	ConnModeValue
 	 	MAX-ACCESS  read-write
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the direction of this logical connection. For unidirection, traffic goes
				from 'from' to 'to'. Default value is bidirection"
 	 	::= { gslamAGLogicalConnectionEntry 4 }

	gslamAGLogicalConnectionCktID		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The connection ID management application using for creating this connection, Return empty string if not available."
	    ::= { gslamAGLogicalConnectionEntry 5 }

	
	-- UV 5.0 CHANGE START 
