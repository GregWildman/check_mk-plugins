	-- OCC channel objects
	gslamAGOCCChannelTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGOCCChannelEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the channel info of GSLAM-AG."
	    ::= { ipiGslamAGChannelObjs 3}

	gslamAGOCCChannelEntry OBJECT-TYPE
		SYNTAX  		GslamAGOCCChannelEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in channel table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGShelfIndex, gslamAGIOModuleIndex, gslamAGPortIndex, gslamAGChannelIndex }
	    ::= { gslamAGOCCChannelTable 1 }


	GslamAGOCCChannelEntry ::= SEQUENCE {
		gslamAGOCCChannelIndex		Unsigned32,
		gslamAGOCCChannelType		EquipmentTypeValue,
		gslamAGOCCChannelAction		ActionValue,
		gslamAGOCCChannelPrimaryState	PrimaryStateValue,
		gslamAGOCCChannelSecondaryState	SecondaryStateValue,
		gslamAGOCCChannelProtFunction	ProtectionEntityValue,
		gslamAGOCCChannelExpTrc		OCTET STRING,   -- EXPTRC
		gslamAGOCCChannelTrc		    OCTET STRING,   -- TRC
		gslamAGOCCChannelReceivedTrc	OCTET STRING,	-- INCTRC
		gslamAGOCCChannelSFThreshold	Unsigned32,     -- SFTH
		gslamAGOCCChannelSDThreshold	Unsigned32,     -- SDTH
		gslamAGOCCChannelSESThreshold	Unsigned32,     -- SESTH
		gslamAGOCCChannelCSESThreshold Unsigned32, 	-- CSESTH
		gslamAGOCCChannelDegMonLev 	Unsigned32,     -- DEGM
		gslamAGOCCChannelDegThreshold  Integer32,	    -- DEGTHR
		gslamAGOCCChannelOCIProtSwitch FlagValue,	    -- OCIPS
		gslamAGOCCChannelTIMDetectMode TIMModeValue,   -- TIMDETM
		gslamAGOCCChannelTIMActDisable FlagValue,	-- TIMACTDIS
		gslamAGOCCChannelExpSAPI   	OCTET STRING,	-- EXPSAPI
		gslamAGOCCChannelExpDAPI   	OCTET STRING,   -- EXPDAPI
		gslamAGOCCChannelExpOperSpecified   OCTET STRING,   -- EXPOPR
		gslamAGOCCChannelTrcSAPI   	OCTET STRING,	-- TRCSAPI
		gslamAGOCCChannelTrcDAPI   	OCTET STRING,   -- TRCDAPI
		gslamAGOCCChannelTrcOperSpecified   OCTET STRING,   -- TRCOPR
		gslamAGOCCChannelReceivedTrcSAPI   OCTET STRING,
		gslamAGOCCChannelReceivedTrcDAPI   OCTET STRING,
		gslamAGOCCChannelReceivedTrcOperSpecified   OCTET STRING,
		gslamAGOCCChannelMuxStructure	INTEGER,
		gslamAGOCCChannelDEGAction		FlagValue,
		gslamAGOCCChannelNomTxPwrOCC	OCTET STRING,
		gslamAGOCCChannelITUNumber	Unsigned32,
		gslamAGOCCChannelRxNomOptPwr	OCTET STRING,
		gslamAGOCCChannelTxNomOptPwr	OCTET STRING,
		gslamAGOCCChannelOptPwrCtrl INTEGER,
		gslamAGOCCChannelTxOptPwr	OCTET STRING,
		gslamAGOCCChannelRxOptPwr	OCTET STRING,
        gslamAGOCCChannelPreEmphasisOffset    OCTET STRING
	}

	gslamAGOCCChannelIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index of GSLAM-AG channel table. It uniquely identifies
			each channel. This value is ITU channel number {16-59}
			for an occ channel in an OTS/OPS port"
    	::= { gslamAGOCCChannelEntry 1 }

	gslamAGOCCChannelType	OBJECT-TYPE
		SYNTAX		EquipmentTypeValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicate the equipment type of this GSLAM-AG channel."
	    ::= { gslamAGOCCChannelEntry 2 }

    gslamAGOCCChannelAction     OBJECT-TYPE
	SYNTAX      ActionValue
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicate command issued to this GSLAM-AG channel."
	::= { gslamAGOCCChannelEntry 3 }

	gslamAGOCCChannelPrimaryState	OBJECT-TYPE
		SYNTAX	PrimaryStateValue
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the primary state of this GSLAM-AG channel."
 	 	::= { gslamAGOCCChannelEntry 4 }

	gslamAGOCCChannelSecondaryState	OBJECT-TYPE
		SYNTAX	SecondaryStateValue
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
		    "Indicates the secondary state of this GSLAM-AG channel."
 	 	::= { gslamAGOCCChannelEntry 5 }

	gslamAGOCCChannelProtFunction	OBJECT-TYPE
		SYNTAX	ProtectionEntityValue
		MAX-ACCESS  read-only
		STATUS      current
		DESCRIPTION
			"Indicates the protection function of this GSLAM-AG channel."
		::= { gslamAGOCCChannelEntry 6 }

    gslamAGOCCChannelExpTrc		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Indicates the expected trace value of the GSLAM-AG channel."
		::= { gslamAGOCCChannelEntry 7 }

	gslamAGOCCChannelTrc		OBJECT-TYPE
