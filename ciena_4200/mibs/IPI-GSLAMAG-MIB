	::= { gslamAGChannelEntry 28 }

    gslamAGChannelMuxStructure    OBJECT-TYPE
	SYNTAX      INTEGER {
	    unknown(1),
	    not-present(2),
	    channelized(3),
	    clear(4)
	}
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicates the multiplex structure for an ODUk channel."
	::= { gslamAGChannelEntry 29 }

    gslamAGChannelDEGAction		OBJECT-TYPE
	SYNTAX  FlagValue
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicates the DEG consequent action of this
	    GSLAM-AG channel."
	::= { gslamAGChannelEntry 30 }

    gslamAGChannelCRSLpbkType     OBJECT-TYPE
    SYNTAX      INTEGER {
        unknown(1),
        not-present(2),
        operate(3),
        release(4)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the cross-connect loopback provisioning for ODU2 or OPVCX channel."
    ::= { gslamAGChannelEntry 31 }

    gslamAGChannelCRSLpbkDuration     OBJECT-TYPE
    SYNTAX  Unsigned32 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the duration of cross-connect loopback."
    ::= { gslamAGChannelEntry 32 }

	gslamAGChannelShapingState		OBJECT-TYPE
		SYNTAX		FlagValue
	    MAX-ACCESS  read-write
		STATUS		current
		DESCRIPTION
			"Indicate the shaping state of channel."
	    ::= { gslamAGChannelEntry 33 }

	gslamAGChannelShapingRate		OBJECT-TYPE
		SYNTAX		INTEGER { not-applicable(0), rate-10m(10000),rate-100m(100000),rate-1000m(1000000) }
	    MAX-ACCESS  read-write
		STATUS		current
		DESCRIPTION
			"Indicate the shaping rate of channel."
	    ::= { gslamAGChannelEntry 34 }

-- ************** ESOM SUPPORT STARTS **************
	
	gslamAGChannelLocalExtn           OBJECT-TYPE
		SYNTAX          INTEGER 
								{
								not-applicable(0),
								zero(1),
								ten(2),
								twenty(3),
								thirty(4),
								forty(5),
								fifty(6),
								sixty(7),
								seventy(8),
								eighty(9),
								ninety(10),
								hundred(11)
								}
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION
			"Indicates the maximum allowed distance b/w CN4200 client port and directly attached equipment"
		::= { gslamAGChannelEntry 35 }

	gslamAGChannelNwExtn           OBJECT-TYPE
		SYNTAX          INTEGER {
					not-applicable(1),
					normal(2),
					maximum(3)
					}
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION
			"Indicates the network distance extension mode"
		::= { gslamAGChannelEntry 36 }

	gslamAGChannelCompression           OBJECT-TYPE
			SYNTAX        	FlagValue  
			MAX-ACCESS      read-write
			STATUS          current
			DESCRIPTION
				"Provision the compression datapath option against the CAG channel"
		::= { gslamAGChannelEntry 37 }

	gslamAGChannelNDP		OBJECT-TYPE
		SYNTAX        	FlagValue 
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION
			"Indicate the state of NDP on the specific network optical interface"
		::= { gslamAGChannelEntry 38 }


	gslamAGChannelOSPF		OBJECT-TYPE
		SYNTAX        	FlagValue 
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION
			"Indicate the state of the OSPF Protocol for the interface port"
		::= { gslamAGChannelEntry 39 }

	gslamAGChannelGroupSize           OBJECT-TYPE
			SYNTAX          INTEGER
					{
					not-applicable(1),
					sts48c(2),
					sts12c(3),
					sts3c(4),
					vc4-16c(5),
					vc4-4c(6),
					vc4(7)
					}
			MAX-ACCESS      read-write
			STATUS          current
			DESCRIPTION
				"Indicates the number of concatenated sts-1/vc4, default is zero"
		::= { gslamAGChannelEntry 40 }
	
	-- ************** ESOM SUPPORT ENDS **************
	
	-- UV 5.0 START

	-- channel objects
	gslamAGREL50ChannelTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGREL50ChannelEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the channel info of GSLAM-AG."
	    ::= { ipiGslamAGChannelObjs 2}

	gslamAGREL50ChannelEntry OBJECT-TYPE
		SYNTAX  		GslamAGREL50ChannelEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in channel table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGREL50ShelfIndex, gslamAGREL50IOModuleIndex, gslamAGREL50PortIndex, gslamAGREL50ChannelIndex }
	    ::= { gslamAGREL50ChannelTable 1 }


	GslamAGREL50ChannelEntry ::= SEQUENCE {
		gslamAGREL50ChannelIndex		Unsigned32,
		gslamAGREL50ChannelType		EquipmentTypeValue,
		gslamAGREL50ChannelAction		ActionValue,
		gslamAGREL50ChannelPrimaryState	PrimaryStateValue,
		gslamAGREL50ChannelSecondaryState	SecondaryStateValue,
		gslamAGREL50ChannelProtFunction	ProtectionEntityValue,
		gslamAGREL50ChannelExpTrc		OCTET STRING,   -- EXPTRC
		gslamAGREL50ChannelTrc		    OCTET STRING,   -- TRC
		gslamAGREL50ChannelReceivedTrc	OCTET STRING,	-- INCTRC
		gslamAGREL50ChannelSFThreshold	Unsigned32,     -- SFTH
		gslamAGREL50ChannelSDThreshold	Unsigned32,     -- SDTH
		gslamAGREL50ChannelSESThreshold	Unsigned32,     -- SESTH
		gslamAGREL50ChannelCSESThreshold Unsigned32, 	-- CSESTH
		gslamAGREL50ChannelDegMonLev 	Unsigned32,     -- DEGM
		gslamAGREL50ChannelDegThreshold  Integer32,	    -- DEGTHR
		gslamAGREL50ChannelOCIProtSwitch FlagValue,	    -- OCIPS
		gslamAGREL50ChannelTIMDetectMode TIMModeValue,   -- TIMDETM
		gslamAGREL50ChannelTIMActDisable FlagValue,	-- TIMACTDIS
		gslamAGREL50ChannelExpSAPI   	OCTET STRING,	-- EXPSAPI
		gslamAGREL50ChannelExpDAPI   	OCTET STRING,   -- EXPDAPI
		gslamAGREL50ChannelExpOperSpecified   OCTET STRING,   -- EXPOPR
		gslamAGREL50ChannelTrcSAPI   	OCTET STRING,	-- TRCSAPI
		gslamAGREL50ChannelTrcDAPI   	OCTET STRING,   -- TRCDAPI
		gslamAGREL50ChannelTrcOperSpecified   OCTET STRING,   -- TRCOPR
		gslamAGREL50ChannelReceivedTrcSAPI   OCTET STRING,
		gslamAGREL50ChannelReceivedTrcDAPI   OCTET STRING,
		gslamAGREL50ChannelReceivedTrcOperSpecified   OCTET STRING,
		gslamAGREL50ChannelMuxStructure	INTEGER,
		gslamAGREL50ChannelDEGAction		FlagValue,
		gslamAGREL50ChannelNomTxPwrOCC	OCTET STRING,
		gslamAGREL50ChannelITUNumber	Unsigned32,
		gslamAGREL50ChannelRxNomOptPwr	OCTET STRING,
		gslamAGREL50ChannelTxNomOptPwr	OCTET STRING,
		gslamAGREL50ChannelOptPwrCtrl INTEGER,
		gslamAGREL50ChannelTxOptPwr OCTET STRING,
		gslamAGREL50ChannelRxOptPwr OCTET STRING,
        gslamAGREL50ChannelPreEmphasisOffset OCTET STRING,
        gslamAGREL50ChannelCRSLpbkType       INTEGER,
        gslamAGREL50ChannelCRSLpbkDuration   Unsigned32,
		gslamAGREL50ChannelShapingState		FlagValue,
		gslamAGREL50ChannelShapingRate		INTEGER,
        gslamAGREL50ChannelLocalExtn	INTEGER,
		gslamAGREL50ChannelNwExtn	INTEGER,
		gslamAGREL50ChannelCompression	FlagValue,
		gslamAGREL50ChannelNDP		FlagValue,
		gslamAGREL50ChannelOSPF		FlagValue,
		gslamAGREL50ChannelGroupSize	INTEGER        
	}

	gslamAGREL50ChannelIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index of GSLAM-AG channel table. It uniquely identifies
			each channel. This value is ITU channel number {16-59}
			for an occ channel in an OTS/OPS port"
    	::= { gslamAGREL50ChannelEntry 1 }

	gslamAGREL50ChannelType	OBJECT-TYPE
		SYNTAX		EquipmentTypeValue
