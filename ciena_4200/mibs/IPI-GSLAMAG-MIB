	-- ************** ESOM SUPPORTS ENDS **************

 -- Subtending up-port table
    gslamAGUPPortTable                OBJECT-TYPE
        SYNTAX          SEQUENCE OF GslamAGUPPortEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
             "A table about the Subtending port info of GSLAM-AG."
        ::= { ipiGslamAGPortObjs 4 }

    gslamAGUPPortEntry OBJECT-TYPE
        SYNTAX                  GslamAGUPPortEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
             "A row in the Subtending port table.
              Entries can't be deleted or created via SNMP ops."
        INDEX   { gslamAGREL50ShelfIndex, gslamAGUPPortIndex }
        ::= { gslamAGUPPortTable 1 }

    GslamAGUPPortEntry ::= SEQUENCE {
        gslamAGUPPortIndex       Unsigned32
    }

    gslamAGUPPortIndex  OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only 
        STATUS      current
        DESCRIPTION
            "Indicates the port index for an Subtending"
    ::= { gslamAGUPPortEntry 1 }

 -- Subtending-down port table
    gslamAGDOWNPortTable                OBJECT-TYPE
        SYNTAX          SEQUENCE OF GslamAGDOWNPortEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
             "A table about the Subtending port info of GSLAM-AG."
        ::= { ipiGslamAGPortObjs 5 }

    gslamAGDOWNPortEntry OBJECT-TYPE
        SYNTAX                  GslamAGDOWNPortEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
             "A row in the Subtending port table.
              Entries can't be deleted or created via SNMP ops."
        INDEX   { gslamAGREL50ShelfIndex, gslamAGDOWNPortIndex }
        ::= { gslamAGDOWNPortTable 1 }

    GslamAGDOWNPortEntry ::= SEQUENCE {
        gslamAGDOWNPortIndex         Unsigned32
    }

    gslamAGDOWNPortIndex    OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only 
        STATUS      current
        DESCRIPTION
            "Indicates the port index for an Subtending"
    ::= { gslamAGDOWNPortEntry 1 }

	-- UV 5.0 END

	-- channel objects
	gslamAGChannelTable		OBJECT-TYPE
		SYNTAX		SEQUENCE OF GslamAGChannelEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"A table about the channel info of GSLAM-AG."
	    ::= { ipiGslamAGChannelObjs 1 }

	gslamAGChannelEntry OBJECT-TYPE
		SYNTAX  		GslamAGChannelEntry
		MAX-ACCESS	not-accessible
		STATUS  		current
		DESCRIPTION
			"A row in channel table.
			Entries can't be deleted or created via SNMP ops."
		INDEX   { gslamAGShelfIndex, gslamAGIOModuleIndex, gslamAGPortIndex, gslamAGChannelIndex }
	    ::= { gslamAGChannelTable 1 }


	GslamAGChannelEntry ::= SEQUENCE {
		gslamAGChannelIndex		Unsigned32,
		gslamAGChannelIfIndex	Unsigned32,
		gslamAGChannelType		EquipmentTypeValue,
		gslamAGChannelAction		ActionValue,
		gslamAGChannelPrimaryState	PrimaryStateValue,
		gslamAGChannelSecondaryState	SecondaryStateValue,
		gslamAGChannelProtFunction	ProtectionEntityValue,
		gslamAGChannelExpTrc		OCTET STRING,   -- EXPTRC
		gslamAGChannelTrc		    OCTET STRING,   -- TRC
		gslamAGChannelReceivedTrc	OCTET STRING,	-- INCTRC
		gslamAGChannelSFThreshold	Unsigned32,     -- SFTH
		gslamAGChannelSDThreshold	Unsigned32,     -- SDTH
		gslamAGChannelSESThreshold	Unsigned32,     -- SESTH
		gslamAGChannelCSESThreshold Unsigned32, 	-- CSESTH
		gslamAGChannelDegMonLev 	Unsigned32,     -- DEGM
		gslamAGChannelDegThreshold  Integer32,	    -- DEGTHR
		gslamAGChannelOCIProtSwitch FlagValue,	    -- OCIPS
		gslamAGChannelTIMDetectMode TIMModeValue,   -- TIMDETM
		gslamAGChannelTIMActDisable FlagValue,	-- TIMACTDIS
		gslamAGChannelExpSAPI   	OCTET STRING,	-- EXPSAPI
		gslamAGChannelExpDAPI   	OCTET STRING,   -- EXPDAPI
		gslamAGChannelExpOperSpecified   OCTET STRING,   -- EXPOPR
		gslamAGChannelTrcSAPI   	OCTET STRING,	-- TRCSAPI
		gslamAGChannelTrcDAPI   	OCTET STRING,   -- TRCDAPI
		gslamAGChannelTrcOperSpecified   OCTET STRING,   -- TRCOPR
		gslamAGChannelReceivedTrcSAPI   OCTET STRING,
		gslamAGChannelReceivedTrcDAPI   OCTET STRING,
		gslamAGChannelReceivedTrcOperSpecified   OCTET STRING,
		gslamAGChannelMuxStructure	INTEGER,
		gslamAGChannelDEGAction		FlagValue,
        gslamAGChannelCRSLpbkType       INTEGER,
        gslamAGChannelCRSLpbkDuration   Unsigned32,
        gslamAGChannelShapingState		FlagValue,
		gslamAGChannelShapingRate		INTEGER,
		gslamAGChannelLocalExtn		INTEGER,
		gslamAGChannelNwExtn		INTEGER,
		gslamAGChannelCompression	FlagValue,
		gslamAGChannelNDP		FlagValue,
		gslamAGChannelOSPF		FlagValue,
		gslamAGChannelGroupSize		INTEGER
	}

	gslamAGChannelIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Index of GSLAM-AG channel table. It uniquely identifies
			each channel."
    	::= { gslamAGChannelEntry 1 }

	gslamAGChannelIfIndex	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicate the ifIndex of Mib2's interface table of this
			GSLAM-AG channel."
	    ::= { gslamAGChannelEntry 2 }

	gslamAGChannelType	OBJECT-TYPE
		SYNTAX		EquipmentTypeValue
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"Indicate the equipment type of this GSLAM-AG channel."
	    ::= { gslamAGChannelEntry 3 }

    gslamAGChannelAction     OBJECT-TYPE
	SYNTAX      ActionValue
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicate command issued to this GSLAM-AG channel."
	::= { gslamAGChannelEntry 4 }

	gslamAGChannelPrimaryState	OBJECT-TYPE
		SYNTAX	PrimaryStateValue
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
			"Indicates the primary state of this GSLAM-AG channel."
 	 	::= { gslamAGChannelEntry 5 }

	gslamAGChannelSecondaryState	OBJECT-TYPE
		SYNTAX	SecondaryStateValue
 	 	MAX-ACCESS  read-only
 	 	STATUS      current
 	 	DESCRIPTION
		    "Indicates the secondary state of this GSLAM-AG channel."
 	 	::= { gslamAGChannelEntry 6 }

	gslamAGChannelProtFunction	OBJECT-TYPE
		SYNTAX	ProtectionEntityValue
		MAX-ACCESS  read-only
		STATUS      current
		DESCRIPTION
			"Indicates the protection function of this GSLAM-AG channel."
		::= { gslamAGChannelEntry 7 }

    gslamAGChannelExpTrc		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Indicates the expected trace value of the GSLAM-AG channel."
		::= { gslamAGChannelEntry 8 }

	gslamAGChannelTrc		OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(0..64))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"Indicates the transmitted trace value of the GSLAM-AG
			channel."
		::= { gslamAGChannelEntry 9 }

	gslamAGChannelReceivedTrc     OBJECT-TYPE
	SYNTAX      OCTET STRING(SIZE(0..64))
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	    "Indicates the received trace value of the GSLAM-AG
	    channel."
	::= { gslamAGChannelEntry 10 }

    gslamAGChannelSFThreshold		OBJECT-TYPE
		SYNTAX	Unsigned32
		MAX-ACCESS  read-write
		STATUS      current
		DESCRIPTION
			"Indicates the signal fail threshold value of this
			GSLAM-AG channel."
		::= { gslamAGChannelEntry 11 }

    gslamAGChannelSDThreshold		OBJECT-TYPE
		SYNTAX	Unsigned32
		MAX-ACCESS  read-write
		STATUS      current
		DESCRIPTION
		    "Indicates the signal degrade threshold value of this
			GSLAM-AG channel."
		::= { gslamAGChannelEntry 12 }

	gslamAGChannelSESThreshold		OBJECT-TYPE
		SYNTAX	Unsigned32
		MAX-ACCESS  read-write
		STATUS      current
		DESCRIPTION
			"Indicates the severely errored seconds declaration
			threshold value of this GSLAM-AG channel."
	    ::= { gslamAGChannelEntry 13 }

    gslamAGChannelCSESThreshold     OBJECT-TYPE
	SYNTAX  Unsigned32
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicates the consecutive signal errored seconds
	    declaration threshold value of this GSLAM-AG channel."
	::= { gslamAGChannelEntry 14 }

    gslamAGChannelDegMonLev OBJECT-TYPE
	SYNTAX  Unsigned32
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicates the degrade monitored level value of this
	    GSLAM-AG channel."
	::= { gslamAGChannelEntry 15 }

    gslamAGChannelDegThreshold  OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	    "Indicates the degrade threshold value (percentage of
	    errored blocks) of this GSLAM-AG channel. -1 mean not applicable"
	::= { gslamAGChannelEntry 16 }

    gslamAGChannelOCIProtSwitch OBJECT-TYPE
	SYNTAX  FlagValue
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
