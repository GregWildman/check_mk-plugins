	-- GslamAG notification helper objects
	cn4200NotificationHelperPEOID	OBJECT-TYPE
		SYNTAX		OBJECT IDENTIFIER
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Used in the variable binding of some gslamAG notification to indicate the OID of
			 the physical entity OR it is the IOD of user for security related alarms involved
			 in the GSLAM-AG notification."
	    ::= { cn4200NotificationHelperObjs 1 }

	cn4200NotificationHelperPEType	OBJECT-TYPE
		SYNTAX		EquipmentTypeValue
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Used in the variable binding of some gslamAG notification to indicate the type of
			 the physical entity involved in the GSLAM-AG notification."
	    ::= { cn4200NotificationHelperObjs 2 }

	cn4200NotificationHelperAction	OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			not-applicable(2),
			none(3),			-- no action associated with the notification
			set(4),			-- alarm/event set
			clear(5),		-- alarm/event clear
			transient(6),	-- no set/clear condition
			clearall(7)		-- clear all set alarm/event for equipment
		}
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Used in the variable binding of some gslamAG notification to indicate the action
			 in the GSLAM-AG notification."
	    ::= { cn4200NotificationHelperObjs 3 }

	cn4200NotificationHelperSev	OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			not-present(2),
			inactive(3),
			not-reported(4),
			not-alarmed(5),
			minor(6),
			major(7),
			critical(8)
		}
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Used in the variable binding of some gslamAG notification to indicate the severity
			 in the GSLAM-AG notification."
	    ::= { cn4200NotificationHelperObjs 4 }

	cn4200NotificationMonTypeAndDir OBJECT-TYPE
		SYNTAX INTEGER {
			unknown(1),
			rx-cvl(2),
